规则("")
{
	事件
	{
		玩家造成最后一击;
		双方;
		全部;
	}

	条件
	{
		数组包含(从数组中移除(全局.b, 所用英雄(全局.P)), 所用英雄(攻击方)) == 真;
		数组包含(全局.b, 所用英雄(被攻击方)) == 真;
		事件技能 != 按钮(近身攻击);
	}

	动作
	{
		If(攻击方 != 全局.N);
			修改玩家分数(攻击方, 攻击方 - 10);
			小字体信息(攻击方, 自定义字符串("{杀害同胞，分数－10,倒地12秒}"));
			设置状态(事件玩家, 无, 击倒, 12);
		End;
		If(被攻击方 == 全局.P);
			修改玩家分数(事件玩家, 事件玩家 - 85);
		End;
	}
}

