规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		事件玩家 == 全局.FS1;
		全局.FS != 0;
		(按钮被按下(事件玩家, 主要攻击模式) == 真 || 按钮被按下(事件玩家, 辅助攻击模式) == 真 || 按钮被按下(事件玩家, 蹲下) == 真) == 真;
		比赛时间 >= 380;
	}

	动作
	{
		If(按钮被按下(事件玩家, 主要攻击模式) == 真);
			事件玩家.GONGJI += 70;
			设置造成伤害(事件玩家, 100 + 事件玩家.GONGJI);
			小字体信息(所有玩家(所有队伍), 自定义字符串("{0}伤害由100%——>170%", 所用英雄(事件玩家)));
		Else If(按钮被按下(事件玩家, 辅助攻击模式) == 真);
			事件玩家.SM += 50;
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
			等待(0.100, 无视条件);
			治疗(事件玩家, 无, 500);
			小字体信息(所有玩家(所有队伍), 自定义字符串("{0}生命值由100%-->150%", 所用英雄(事件玩家)));
			修改玩家分数(事件玩家, 事件玩家 - 5);
		Else If(按钮被按下(事件玩家, 蹲下) == 真);
			事件玩家.SH += 20;
			设置移动速度(事件玩家, 100 + 事件玩家.SH);
			小字体信息(所有玩家(所有队伍), 自定义字符串("{0}速度由100%-->120%", 所用英雄(事件玩家)));
		End;
		修改玩家分数(事件玩家, 事件玩家 - 5);
		消除地图文本(全局.FS);
		播放效果(所有玩家(所有队伍), 有益选择效果 , 红色, 事件玩家, 5);
		播放效果(所有玩家(所有队伍), 正面状态施加声音, 白色, 事件玩家, 40);
		等待(0.100, 无视条件);
		全局.FS = 0;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		莱因哈特;
	}

	条件
	{
		按钮被按下(事件玩家, 跳跃) == 真;
		高度(事件玩家) >= 1.200;
	}

	动作
	{
		等待(0.050, 当为“假”时中止);
		施加推力(事件玩家, 方向(眼睛位置(事件玩家), 眼睛位置(事件玩家) + 矢量(0, 10, 0) + 面朝方向(事件玩家) * 10), 19, 至地图, 取消相反运动);
		小字体信息(事件玩家, 自定义字符串("{二段跳进入冷却7秒}"));
		等待(7, 无视条件);
		小字体信息(事件玩家, 自定义字符串("二段跳准备就绪"));
	}
}

