		调用子程序(Sub);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		天使;
	}

	条件
	{
		事件玩家.XUANZHE > 0;
		存活(事件玩家) == 真;
		按钮被按下(事件玩家, 技能2) == 真;
		技能冷却时间(事件玩家, 按钮(技能2)) == 0;
	}

	动作
	{
		If(事件玩家.XUANZHE == 1);
			设置技能冷却(事件玩家, 按钮(技能2), 30);
			播放效果(事件玩家, 有益选择效果 , 黄色, 事件玩家, 3);
			播放效果(事件玩家, 环状爆炸声音, 黄色, 事件玩家, 100);
			播放效果(事件玩家, 有益爆炸, 黄色, 事件玩家, 5);
			治疗(事件玩家, 无, 140);
			治疗(选择英雄的玩家(英雄(托比昂), 所有队伍), 无, 140);
			设置移动速度(事件玩家, 事件玩家.SH + 160);
			设置移动速度(选择英雄的玩家(英雄(托比昂), 所有队伍), 事件玩家.SH + 160);
			小字体信息(选择英雄的玩家(英雄(托比昂), 所有队伍), 字符串("{0} {1}", 技能图标字符串(英雄(天使), 按钮(技能2)), 自定义字符串("回血140，并加速60%持续3秒")));
			等待(3, 无视条件);
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			设置移动速度(选择英雄的玩家(英雄(托比昂), 所有队伍), 事件玩家.SH + 100);
		Else If(事件玩家.XUANZHE == 2);
			设置状态(事件玩家, 无, 无敌, 3);
			创建效果(所有玩家(所有队伍), 球体, 黄色, 事件玩家, 2, 可见，位置和半径);
			事件玩家.XIAOG1 = 最后创建的实体;
			设置技能冷却(事件玩家, 按钮(技能2), 20);
			播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 200);
			等待(3, 无视条件);
			消除效果(事件玩家.XIAOG1);
		End;
	}
}

