规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		末日铁拳;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		事件玩家.GONGYONG >= 全局.JYZ;
	}

	动作
	{
		全局.JYZ += 575;
		播放效果(所有玩家(所有队伍), 正面状态施加声音, 白色, 事件玩家, 60);
		播放效果(所有玩家(所有队伍), 有益选择效果 , 白色, 事件玩家, 15);
		事件玩家.SM += 15;
		事件玩家.GONGJI += 30;
		设置最大生命值(事件玩家, 事件玩家.SM + 85);
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 70);
		事件玩家.GONGYONG = 0;
		事件玩家.b += 1;
		等待(0.100, 无视条件);
		治疗(事件玩家, 无, 9999);
		小字体信息(事件玩家, 字符串("{0} {1} {2}", 英雄图标字符串(英雄(末日铁拳)), 自定义字符串("当前等级："), 事件玩家.b));
	}
}

规则("")
{
	事件
