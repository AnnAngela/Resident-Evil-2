		If(事件玩家.b == 6);
			设置技能冷却(事件玩家, 按钮(技能1), 6);
		Else If(事件玩家.b == 5);
			设置技能冷却(事件玩家, 按钮(技能1), 10);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		查莉娅;
	}

	条件
	{
		存活(事件玩家) == 真;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		If(事件玩家.b == 2);
			事件玩家.GONGYONG = 射线命中位置(眼睛位置(事件玩家), 眼睛位置(事件玩家) + 面朝方向(事件玩家) * 25, 无, 无, 假);
			播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 60);
			小字体信息(事件玩家, 自定义字符串("酸性毒液进入冷却22秒"));
			创建效果(所有玩家(所有队伍), 球, 绿色, 事件玩家.a, 2, 可见，位置和半径);
			事件玩家.XIAOG1 = 最后创建的实体;
			事件玩家.a = 眼睛位置(事件玩家);
			持续追踪玩家变量(事件玩家, a, 事件玩家.GONGYONG, 1.700, 终点及持续时间);
			等待(1.700, 无视条件);
			消除效果(事件玩家.XIAOG1);
			播放效果(所有玩家(所有队伍), 有害爆炸, 绿色, 事件玩家.a, 6);
			播放效果(所有玩家(所有队伍), 环状爆炸, 绿色, 事件玩家.a, 6);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家.a, 6);
			播放效果(所有玩家(所有队伍), 负面状态施加声音, 白色, 事件玩家.a, 120);
			伤害(范围内玩家(事件玩家.a, 6, 所有队伍, 关闭), 事件玩家, 195);
			设置状态(范围内玩家(事件玩家.a, 6, 所有队伍, 关闭), 无, 昏迷, 2);
			停止追踪玩家变量(事件玩家, a);
			等待(22, 无视条件);
			小字体信息(事件玩家, 自定义字符串("酸性毒液冷却就绪,可再次使用"));
			播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 80);
		Else If(事件玩家.b == 4);
			消除地图文本(事件玩家.WENBEN);
			全局.D = 数组随机取值(从数组中移除(从数组中移除(全局.RM, 英雄(布丽吉塔)), 英雄(安娜)));
			设置主要攻击模式启用(事件玩家, 假);
			设置辅助攻击模式启用(事件玩家, 假);
			设置启用技能 1(事件玩家, 假);
			设置启用技能 2(事件玩家, 假);
			禁用按钮(事件玩家, 终极技能);
			设置造成伤害(事件玩家, 405);
			设置最大生命值(事件玩家, 175);
			等待(0.250, 无视条件);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(回声), 按钮(终极技能)), 自定义字符串("已随机切换至人类身份")));
			修改全局变量(b, 添加至数组, 全局.D);
			修改全局变量(a, 根据值从数组中移除, 全局.D);
			创建地图文本(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.b, 所用英雄(当前数组元素))), 自定义字符串("诡秘感染体"), 事件玩家, 2, 根据表面截取, 可见，位置和字符串, 红色, 默认可见度);
			开始强制玩家选择英雄(事件玩家, 全局.D);
		Else If(事件玩家.b == 5 && 数组包含(全局.a, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) == 真 && 数组包含(范围内玩家(事件玩家, 4, 所有队伍, 表面), 距离准星最近的玩家(事件玩家, 所有队伍)) == 真);
			小字体信息(事件玩家, 自定义字符串("已致命一击,技能进入冷却15秒"));
			事件玩家.GONGYONG = 距离准星最近的玩家(事件玩家, 所有队伍);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 红色, 眼睛位置(事件玩家.GONGYONG), 2);
			播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 120);
			伤害(事件玩家.GONGYONG, 事件玩家, 48);
			设置状态(事件玩家.GONGYONG, 事件玩家, 昏迷, 0.700);
			等待(15, 无视条件);
			小字体信息(事件玩家, 自定义字符串("致命一击冷却就绪,可再次释放"));
		End;
	}
}

规则("")
{
	事件
	{
		玩家阵亡;
		双方;
		安娜;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		事件玩家.GONGYONG == 5;
	}

