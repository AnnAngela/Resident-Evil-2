规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		破坏球;
	}

	条件
	{
		事件技能 != 按钮(技能1);
	}

	动作
	{
		If(事件技能 == 按钮(蹲下));
			设置状态(被攻击方, 事件玩家, 昏迷, 1);
		Else If(事件技能 == 按钮(近身攻击));
			伤害(被攻击方, 事件玩家, 95);
		Else If(事件技能 == 按钮(终极技能));
			伤害(被攻击方, 事件玩家, 125);
		End;
	}
}

