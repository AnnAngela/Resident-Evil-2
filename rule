		修改全局变量(wenzi5, 根据值从数组中移除, 事件玩家);
		If(所用英雄(事件玩家) == 英雄(路霸));
			修改全局变量(bianshen, 添加至数组, 事件玩家.b);
		Else If(所用英雄(事件玩家) == 英雄(查莉娅));
			修改全局变量(bianshen1, 添加至数组, 事件玩家.b);
		End;
		消除HUD文本(事件玩家.HUB);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		源氏;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		设置受到伤害(事件玩家, 50);
		设置主要攻击模式启用(事件玩家, 假);
		设置辅助攻击模式启用(事件玩家, 假);
		创建效果(所有玩家(所有队伍), 有害光环, 红色, 事件玩家, 2, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 130);
		播放效果(所有玩家(所有队伍), 环状爆炸, 红色, 事件玩家, 5);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 红色, 事件玩家, 5);
		事件玩家.GONGYONG = 6;
		小字体信息(事件玩家, 自定义字符串("影杀开启,持续8秒"));
		设置技能冷却(事件玩家, 按钮(技能1), 0);
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 480);
		等待(8, 无视条件);
		事件玩家.GONGYONG = 0;
		小字体信息(事件玩家, 自定义字符串("影杀效果结束,技能进入冷却15秒"));
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		播放效果(事件玩家, 状态爆炸声音, 白色, 事件玩家, 100);
		消除效果(事件玩家.XIAOG1);
		设置受到伤害(事件玩家, 100);
		设置主要攻击模式启用(事件玩家, 真);
		设置辅助攻击模式启用(事件玩家, 真);
		等待(15, 无视条件);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		小字体信息(事件玩家, 自定义字符串("影杀可再次释放"));
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		布丽吉塔;
	}

	条件
	{
