规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		卢西奥;
	}

	条件
	{
		在墙上(事件玩家) == 真;
	}

	动作
	{
		If(终极技能充能百分比(事件玩家) > 0);
			设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) - 0.500);
			等待(0.650, 当为“假”时中止);
			如条件为”真“则循环;
		Else;
			小字体信息(事件玩家, 自定义字符串("充能不足,晕眩自身1秒"));
			设置状态(事件玩家, 无, 昏迷, 1);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		卢西奥;
	}

	条件
	{
		在墙上(事件玩家) == 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("划墙会消耗大招充能"));
		等待(5, 无视条件);
	}
