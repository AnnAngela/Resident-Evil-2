规则("")
{
	事件
	{
		玩家造成治疗;
		双方;
		安娜;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件技能 == 按钮(技能2);
	}

	动作
	{
		事件玩家.SH += 40;
		设置移动速度(事件玩家, 事件玩家.SH + 100);
		小字体信息(事件玩家, 自定义字符串("已加速40%持续2.5秒"));
		等待(0.680, 无视条件);
		设置技能冷却(事件玩家, 按钮(技能2), 5.320);
		等待(1.820, 无视条件);
		事件玩家.SH -= 40;
		设置移动速度(事件玩家, 事件玩家.SH + 100);
	}
