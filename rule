规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		黑百合;
	}

	条件
	{
		(事件玩家.XUANZHE == 2 || 事件玩家.XUANZHE == 3) == 真;
		技能冷却时间(事件玩家, 按钮(技能1)) > 3;
	}

	动作
	{
		If(事件玩家.XUANZHE == 2);
			设置技能冷却(事件玩家, 按钮(技能1), 2.200);
		Else;
			事件玩家.SH += 50;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			小字体信息(事件玩家, 自定义字符串("速度已增加"));
			等待(3, 无视条件);
			事件玩家.SH -= 50;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		巴蒂斯特;
	}

	条件
	{
		正在使用终极技能(事件玩家) == 真;
	}

	动作
	{
		全局.BD = 射线命中位置(眼睛位置(事件玩家), 眼睛位置(事件玩家) + 面朝方向(事件玩家) * 30, 无, 事件玩家, 真);
		创建效果(所有玩家(所有队伍), 环, 黄色, 全局.BD, 3, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		If(事件玩家.XUANZHE == 2);
			创建地图文本(所有玩家(所有队伍), 自定义字符串("传送门"), 全局.BD, 2, 根据表面截取, 可见，位置和字符串, 绿色, 默认可见度);
		Else;
			创建地图文本(所有玩家(所有队伍), 自定义字符串("加速门"), 全局.BD, 2, 根据表面截取, 可见，位置和字符串, 绿色, 默认可见度);
		End;
		事件玩家.WENBEN = 上一个文本ID;
		等待(7, 无视条件);
		消除效果(事件玩家.XIAOG1);
		消除地图文本(事件玩家.WENBEN);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		正在使用终极技能(选择英雄的玩家(英雄(巴蒂斯特), 所有队伍)) == 真;
		数组包含(范围内玩家(全局.BD, 3, 所有队伍, 关闭), 事件玩家) == 真;
		数组包含(全局.a, 所用英雄(事件玩家)) == 真;
		数组包含(全局.RL, 所用英雄(事件玩家)) != 真;
	}

	动作
	{
		If(选择英雄的玩家(英雄(巴蒂斯特), 所有队伍).XUANZHE == 2);
			重生(事件玩家);
			等待(1, 无视条件);
		Else;
			小字体信息(事件玩家, 自定义字符串("穿过了传送门，你获得了70%加速效果持续3秒"));
			事件玩家.SH += 70;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			等待(3, 无视条件);
			事件玩家.SH -= 70;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		堡垒;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		终极技能充能百分比(事件玩家) == 0;
	}

	动作
	{
		设置终极技能充能(事件玩家, 100);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		堡垒;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 终极技能) == 真;
	}

	动作
	{
		事件玩家.a += 1;
		If(事件玩家.a == 4 && 事件玩家.W == 1);
			事件玩家.W = 2;
			事件玩家.a = 0;
		Else If(事件玩家.a == 4 && 事件玩家.W == 2);
			事件玩家.W = 1;
			事件玩家.a = 0;
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		堡垒;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件玩家.W == 1;
		按钮被按下(事件玩家, 主要攻击模式) == 真;
		事件玩家.a != 0;
	}

	动作
	{
		If(数组包含(全局.c, 事件玩家.a) == 真);
			If(事件玩家.a == 1);
				修改全局变量(c, 根据值从数组中移除, 1);
				事件玩家.SH += 22;
				设置移动速度(事件玩家, 事件玩家.SH + 100);
			Else If(事件玩家.a == 2);
				事件玩家.SM += 50;
				设置最大生命值(事件玩家, 事件玩家.SM + 100);
				修改全局变量(c, 根据值从数组中移除, 2);
			Else If(事件玩家.a == 3);
				修改全局变量(c, 根据值从数组中移除, 3);
				设置受到伤害(事件玩家, 75);
			End;
			小字体信息(事件玩家, 自定义字符串("已改装完毕"));
			播放效果(所有玩家(所有队伍), 有益选择效果 , 白色, 事件玩家, 3);
			事件玩家.W = 0;
			事件玩家.a = 0;
		Else;
			小字体信息(事件玩家, 自定义字符串("不要重复强化"));
			等待(1, 无视条件);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		堡垒;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件玩家.W == 2;
		按钮被按下(事件玩家, 主要攻击模式) == 真;
		事件玩家.a != 0;
	}

	动作
	{
		If(数组包含(全局.c, 事件玩家.a + 3) == 真);
			If(事件玩家.a == 1);
				修改全局变量(c, 根据值从数组中移除, 4);
				设置造成治疗(事件玩家, 250);
			Else If(事件玩家.a == 2);
				修改全局变量(c, 根据值从数组中移除, 5);
				事件玩家.c = 自定义字符串("使用右键会获得1秒加速");
				事件玩家.WENzi = 自定义字符串("{0}(主动)改装自身零件,冷却60秒\r\n{1}\r\n{2}", 技能图标字符串(英雄(堡垒), 按钮(辅助攻击模式)), 事件玩家.b, 事件玩家.c);
			Else If(事件玩家.a == 3);
				修改全局变量(c, 根据值从数组中移除, 6);
				事件玩家.b = 自定义字符串("近战会击退敌人,冷却2秒");
				事件玩家.WENzi = 自定义字符串("{0}(主动)改装自身零件,冷却60秒\r\n{1}\r\n{2}", 技能图标字符串(英雄(堡垒), 按钮(辅助攻击模式)), 事件玩家.b, 事件玩家.c);
			End;
			小字体信息(事件玩家, 自定义字符串("已改装完毕"));
			播放效果(所有玩家(所有队伍), 有益选择效果 , 白色, 事件玩家, 3);
			事件玩家.W = 0;
			事件玩家.a = 0;
		Else;
			小字体信息(事件玩家, 自定义字符串("不要重复强化"));
			等待(1, 无视条件);
		End;
	}
}

规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		堡垒;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		数组包含(全局.c, 6) != 真;
		数组包含(全局.b, 所用英雄(被攻击方)) == 真;
	}

	动作
	{
		施加推力(被攻击方, 面朝方向(攻击方), 20, 至地图, 取消相反运动);
		播放效果(所有玩家(所有队伍), 有益爆炸, 白色, 眼睛位置(被攻击方), 2);
		等待(0.200, 无视条件);
		设置状态(被攻击方, 事件玩家, 昏迷, 1);
		小字体信息(事件玩家, 自定义字符串("机械重拳进入冷却2秒"));
		等待(2, 无视条件);
		小字体信息(事件玩家, 自定义字符串("机械重拳冷却完毕"));
	}
}

