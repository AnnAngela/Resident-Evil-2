		设置辅助攻击模式启用(所有玩家(所有队伍), 假);
		全局.bianshen = 1;
		修改全局变量(bianshen, 添加至数组, 2);
		修改全局变量(bianshen, 添加至数组, 3);
		修改全局变量(bianshen, 添加至数组, 4);
		修改全局变量(bianshen, 添加至数组, 5);
		修改全局变量(bianshen, 添加至数组, 6);
		等待(0.500, 无视条件);
		全局.bianshen1 = 1;
		修改全局变量(bianshen1, 添加至数组, 2);
		修改全局变量(bianshen1, 添加至数组, 3);
		修改全局变量(bianshen1, 添加至数组, 4);
		修改全局变量(bianshen1, 添加至数组, 5);
		修改全局变量(bianshen, 添加至数组, 7);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		路霸;
	}

	条件
	{
		游戏正在进行中 == 真;
		比赛时间 < 390;
		数组包含(全局.b, 所用英雄(事件玩家)) == 真;
	}

	动作
	{
		事件玩家.b = 数组随机取值(全局.bianshen);
		修改全局变量(bianshen, 根据值从数组中移除, 事件玩家.b);
		等待(0.100, 无视条件);
		If(事件玩家.b == 1);
			事件玩家.WENzi = 自定义字符串("当前职业:(隐匿感染体)\r\n{0}进入隐身状态且下一击造成额外伤害\r\n造成伤害会解除隐身并自我晕眩", 技能图标字符串(英雄(路霸), 按钮(技能2)));
		Else If(事件玩家.b == 2);
			事件玩家.WENzi = 自定义字符串("当前职业:(自爆感染体)\r\n{0}刷新链勾冷却并在2.5秒后会自爆\r\n对周围人类造成200伤害", 技能图标字符串(英雄(路霸), 按钮(技能2)));
		Else If(事件玩家.b == 3);
			事件玩家.WENzi = 自定义字符串("当前职业:(屠夫感染体)\r\n{0}提升自己60%伤害.持续10秒\r\n{1}命中会返还75%冷却", 技能图标字符串(英雄(路霸), 按钮(技能2)), 技能图标字符串(英雄(路霸), 按钮(技能1)));
		Else If(事件玩家.b == 4);
			事件玩家.WENzi = 自定义字符串("当前职业:(无智感染体)\r\n{0}增加25%速度并且无视\r\n减速持续5秒,并获得25%充能\r\n死亡会减少25%充能", 技能图标字符串(英雄(路霸), 按钮(技能2)));
		Else If(事件玩家.b == 5);
			事件玩家.WENzi = 自定义字符串("当前职业:(无畏感染体)\r\n{0}命中人类传送至其身边并获\r\n得无敌5秒,钩子冷却缩短50%", 技能图标字符串(英雄(路霸), 按钮(技能1)));
		Else If(事件玩家.b == 6);
			事件玩家.WENzi = 自定义字符串("当前职业:(鞭笞感染体)\r\n{0}冷却减少至6秒,但不会将人勾过来\r\n命中人类会使其减速60%持续1.5秒", 技能图标字符串(英雄(路霸), 按钮(技能1)));
		Else If(事件玩家.b == 7);
			事件玩家.WENzi = 自定义字符串("当前职业:(跳跳感染体)\r\n受到重力影响大幅降低");
			设置引力(事件玩家, 25);
		End;
	}
}

