规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		莫伊拉;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		终极技能充能百分比(事件玩家) >= 事件玩家.a;
		技能冷却时间(事件玩家, 按钮(技能1)) > 0;
		按钮被按下(事件玩家, 技能1) == 真;
	}

	动作
	{
		设置技能冷却(事件玩家, 按钮(技能1), 0);
		按下按键(事件玩家, 技能1);
		事件玩家.a += 1;
		等待(0.400, 无视条件);
		取消主要动作(事件玩家);
		设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) - 事件玩家.a);
		设置技能冷却(事件玩家, 按钮(技能1), 3);
		小字体信息(事件玩家, 自定义字符串("消散层数+1,需要能量提升至{0}%", 事件玩家.a));
		伤害(范围内玩家(事件玩家, 8, 所有队伍, 关闭), 事件玩家, 事件玩家.a * 45);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 紫色, 事件玩家, 4);
		等待(5.600, 当为“真”时重新开始);
		小字体信息(事件玩家, 自定义字符串("消散层数清零，所需要能量降低为4%"));
		事件玩家.a = 4;
	}
}

