规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		禅雅塔;
	}

	条件
	{
		事件玩家.XUANZHE == 3;
		游戏正在进行中 == 真;
		比赛时间 <= 70;
		正在蹲下(事件玩家) == 真;
		具有状态(事件玩家, 被入侵) != 真;
	}

	动作
	{
		小字体信息(所有玩家(所有队伍), 自定义字符串("{0}正在开启{1}", 英雄图标字符串(英雄(禅雅塔)), 技能图标字符串(英雄(秩序之光), 按钮(技能2))));
		播放效果(所有玩家(所有队伍), 状态爆炸声音, 白色, 事件玩家, 200);
		等待(4, 当为“假”时中止);
		小字体信息(事件玩家, 自定义字符串("{0}开启剩余2秒", 技能图标字符串(英雄(秩序之光), 按钮(技能2))));
		等待(1, 当为“假”时中止);
		小字体信息(事件玩家, 自定义字符串("{0}开启剩余1秒", 技能图标字符串(英雄(秩序之光), 按钮(技能2))));
		等待(1, 当为“假”时中止);
		全局.Y = 所选位置(事件玩家);
		播放效果(所有玩家(所有队伍), 状态爆炸声音, 白色, 事件玩家, 200);
		大字体信息(所有玩家(所有队伍), 自定义字符串("{0}逃生点已开启", 英雄图标字符串(英雄(禅雅塔))));
		小字体信息(事件玩家, 自定义字符串("转移逃生点完毕,分数+4"));
		修改玩家分数(事件玩家, 事件玩家 + 7);
		全局.QR = 真;
		创建效果(所有玩家(所有队伍), 环, 青绿色, 全局.Y, 3, 可见，位置和半径);
		创建地图文本(所有玩家(所有队伍), 技能图标字符串(英雄(秩序之光), 按钮(技能2)), 全局.Y, 6, 不要截取, 可见，位置和字符串, 白色, 默认可见度);
		等待(0.250, 无视条件);
		创建效果(所有玩家(所有队伍), 环, 青绿色, 全局.Y, 3, 可见，位置和半径);
		创建效果(所有玩家(所有队伍), 有益光环, 水绿色, 全局.Y, 2.500, 可见，位置和半径);
		全局.TSD = 真;
	}
}

