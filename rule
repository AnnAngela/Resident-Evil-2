规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		布丽吉塔;
	}

	条件
	{
		数组包含(全局.b, 所用英雄(被攻击方)) == 真;
	}

	动作
	{
		If(事件玩家.XUANZHE == 1);
			If(事件技能 != 按钮(技能1));
				伤害(范围内玩家(被攻击方, 8, 所有队伍, 关闭), 事件玩家, 200);
				设置状态(已过滤的数组(范围内玩家(被攻击方, 8, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 事件玩家, 击倒, 1);
				播放效果(所有玩家(所有队伍), 环状爆炸, 黄色, 事件玩家, 10);
			End;
		Else;
			If(事件技能 == 按钮(技能1));
				事件玩家.GONGYONG = 眼睛位置(被攻击方);
				创建效果(所有玩家(所有队伍), 球体, 黄色, 事件玩家.GONGYONG, 6, 位置和半径);
				事件玩家.XIAOG1 = 最后创建的实体;
				等待(1.500, 无视条件);
				消除效果(事件玩家.XIAOG1);
				伤害(范围内玩家(事件玩家.GONGYONG, 6, 所有队伍, 关闭), 事件玩家, 500);
				设置状态(已过滤的数组(范围内玩家(事件玩家.GONGYONG, 6, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 事件玩家, 昏迷, 2);
				播放效果(所有玩家(所有队伍), 环状爆炸, 黄色, 事件玩家.GONGYONG, 8);
			End;
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		布丽吉塔;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 技能2) == 真;
		事件玩家.b == 0;
	}

	动作
	{
		事件玩家.b = 1;
		治疗(事件玩家, 事件玩家, 140);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 2);
		播放效果(事件玩家, 正面状态施加声音, 白色, 事件玩家, 100);
		设置启用技能 2(事件玩家, 假);
		小字体信息(事件玩家, 自定义字符串("已恢复140生命"));
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		选择英雄的玩家(英雄(安娜), 所有队伍).XUANZHE != 1;
		具有状态(事件玩家, 沉睡) == 真;
		数组包含(从数组中移除(全局.a, 所用英雄(全局.N)), 所用英雄(事件玩家)) == 真;
	}

	动作
	{
		等待(0.500, 无视条件);
		清除状态(事件玩家, 沉睡);
	}
}