		End;
	}
}

规则("")
{
	事件
	{
		子程序;
		Sub;
	}

	动作
	{
		事件玩家.F = 所选位置(攻击方);
		消除效果(事件玩家.d);
		事件玩家.XUANZHE = -1;
		事件玩家.GONGYONG = 0;
		消除效果(事件玩家.XIAOG1);
		消除地图文本(事件玩家.WENBEN);
		大字体信息(事件玩家, 自定义字符串("变异中，剩余3秒"));
		等待(3, 无视条件);
		传送(事件玩家, 事件玩家.F);
		等待(0.100, 无视条件);
		设置移动速度(事件玩家, 100);
		设置最大生命值(事件玩家, 100);
		等待(0.100, 无视条件);
		设置启用技能 1(事件玩家, 真);
		设置引力(事件玩家, 100);
		设置造成伤害(事件玩家, 100);
		设置受到伤害(事件玩家, 100);
		设置造成治疗(事件玩家, 100);
		设置主要攻击模式启用(事件玩家, 假);
		设置辅助攻击模式启用(事件玩家, 假);
		设置不可见(事件玩家, 无);
		If(数量(全局.bianshen) > 0 && 数量(全局.bianshen1) > 0);
			开始强制玩家选择英雄(事件玩家, 数组随机取值(全局.S));
		Else If(数量(全局.bianshen) == 0);
			开始强制玩家选择英雄(事件玩家, 英雄(查莉娅));
		Else If(数量(全局.bianshen1) == 0);
			开始强制玩家选择英雄(事件玩家, 英雄(路霸));
		End;
		停止按下按钮(事件玩家, 主要攻击模式);
		设置启用技能 2(事件玩家, 真);
		清除状态(事件玩家, 无法杀死);
		设置最大重生时间(事件玩家, 6);
		可用按钮(事件玩家, 终极技能);
		停止加速(事件玩家);
		设置弹道速度(事件玩家, 100);
		解除绑定(事件玩家);
		清除状态(事件玩家, 消散);
		停止强制设置玩家位置(事件玩家);
		可用按钮(事件玩家, 跳跃);
		可用按钮(事件玩家, 技能2);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		美;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		存活(事件玩家) == 真;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		播放效果(所有玩家(所有队伍), 状态爆炸声音, 白色, 事件玩家, 120);
		事件玩家.GONGYONG = 眼睛位置(事件玩家);
		创建效果(所有玩家(所有队伍), 球体, 天蓝色, 事件玩家.GONGYONG, 事件玩家.a, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		持续追踪玩家变量(事件玩家, a, 事件玩家.c, 3, 无);
		创建地图文本(所有玩家(所有队伍), 自定义字符串("即将冰封此区域"), 事件玩家.GONGYONG, 3, 根据表面截取, 可见，位置和字符串, 蓝色, 默认可见度);
		事件玩家.WENBEN = 上一个文本ID;
		等待(3, 无视条件);
		设置状态(已过滤的数组(范围内玩家(事件玩家.GONGYONG, 事件玩家.c, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 事件玩家, 冻结, 事件玩家.b);
		小字体信息(事件玩家, 自定义字符串("冰封进入冷却16秒"));
		消除地图文本(事件玩家.WENBEN);
		消除效果(事件玩家.XIAOG1);
		播放效果(所有玩家(所有队伍), 环状爆炸, 蓝色, 事件玩家.GONGYONG, 13);
		播放效果(所有玩家(所有队伍), 状态爆炸声音, 白色, 事件玩家.GONGYONG, 100);
		停止追踪玩家变量(事件玩家, a);
		等待(16, 无视条件);
		小字体信息(事件玩家, 自定义字符串("冰封技能冷却完毕,可再次释放"));
		播放效果(事件玩家, 爆炸声音, 白色, 事件玩家, 75);
		事件玩家.a = 1;
	}
}

