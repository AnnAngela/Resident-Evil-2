规则("可以修改属性，不要修改规则，谢谢")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		数组包含(全局.RL, 所用英雄(事件玩家)) != 真;
		对任意为”真“(添加至数组(添加至数组(添加至数组(添加至数组(添加至数组(全局.TP, 全局.TP1), 全局.TP2), 全局.TP3), 全局.TP4), 全局.TP5), 数组包含(范围内玩家(当前数组元素, 1.200, 所有队伍, 关闭),
			事件玩家) == 真) == 真;
	}

	动作
	{
		If(数组包含(范围内玩家(全局.TP, 1.200, 所有队伍, 关闭), 事件玩家) == 真);
			传送(事件玩家, 全局.TP1);
		Else If(数组包含(范围内玩家(全局.TP1, 1.200, 所有队伍, 关闭), 事件玩家) == 真);
			传送(事件玩家, 全局.TP);
		Else If(数组包含(范围内玩家(全局.TP2, 1.200, 所有队伍, 关闭), 事件玩家) == 真);
			传送(事件玩家, 全局.TP3);
		Else If(数组包含(范围内玩家(全局.TP3, 1.200, 所有队伍, 关闭), 事件玩家) == 真);
			传送(事件玩家, 全局.TP2);
		Else If(数组包含(范围内玩家(全局.TP4, 1.200, 所有队伍, 关闭), 事件玩家) == 真);
			传送(事件玩家, 全局.TP5);
		Else If(数组包含(范围内玩家(全局.TP5, 1.200, 所有队伍, 关闭), 事件玩家) == 真);
			传送(事件玩家, 全局.TP4);
		End;
		等待(1, 无视条件);
	}
}

