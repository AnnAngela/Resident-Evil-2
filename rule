规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		游戏正在进行中 == 真;
		数组包含(从数组中移除(全局.a, 英雄(托比昂)), 所用英雄(事件玩家)) == 真;
		事件玩家.XUANZHE == 0;
		按钮被按下(事件玩家, 主要攻击模式) == 真;
		按钮被按下(事件玩家, 辅助攻击模式) != 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("正在选择职业中···①"));
		等待(1.500, 当为“假”时中止);
		事件玩家.XUANZHE = 1;
		设置主要攻击模式启用(事件玩家, 真);
		设置辅助攻击模式启用(事件玩家, 真);
		If(所用英雄(事件玩家) == 英雄(秩序之光));
			事件玩家.WENzi = 自定义字符串(
				"术法·移(主动):(蹲下+互动)在自身处创造\r\n传送阵,2秒后按下(shift)将周围人类随机\r\n传送至安全位置并满血.冷却使用后20秒\r\n术法·裂(被动):大招中点处的感染体受到\r\n1000点伤害并晕眩2秒");
			事件玩家.a = 0.500;
		Else If(所用英雄(事件玩家) == 英雄(天使));
			事件玩家.WENzi = 自定义字符串(
				"{0}会跟鼠标最近的托比昂(可穿墙,无视距离)换位\r\n奴役术(被动):对路霸毛妹的近战攻击会将他们变\r\n为变为你的随从,消耗15%生命值上限,冷却40秒 \r\n{1}会另随从与你回血并加速3秒\r\n一体(被动):托比昂逃生后你也会逃生",
				技能图标字符串(英雄(天使), 按钮(技能1)), 技能图标字符串(英雄(天使), 按钮(技能2)));
			设置终极技能充能(事件玩家, 100);
		Else If(所用英雄(事件玩家) == 英雄(美));
			事件玩家.WENzi = 自定义字符串("冰风暴(主动):召唤一场冰风暴缓慢前进,在\r\n风暴内的感染体会被冻结3秒,冷却20秒\r\n{0}结束后冰冻周围感染体1.3秒\r\n霜盾(被动):被技能攻击会冰冻攻击者1.4秒", 技能图标字符串(英雄(美),
				按钮(技能1)));
			事件玩家.GONGYONG = 矢量(0, -50, 0);
		Else If(所用英雄(事件玩家) == 英雄(回声));
			事件玩家.SH += 20;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			事件玩家.WENzi = 自定义字符串("迷惑(主动):在鼠标最近的墙体上创造一个误导性的\r\n存活中迷惑可透视感染体只能存在一个.冷却20秒\r\n{0}只能复制人类,复制感染体会死亡", 技能图标字符串(英雄(回声), 按钮(终极技能)));
		Else If(所用英雄(事件玩家) == 英雄(麦克雷));
			创建地图文本(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TOUSHI, 所用英雄(当前数组元素)) && 事件玩家.GONGYONG != 1), 自定义字符串("存活中"), 事件玩家, 1.500, 不要截取, 可见，位置和字符串, 青绿色,
				默认可见度);
			事件玩家.TOUSHI = 上一个文本ID;
			设置终极技能充能(事件玩家, 95);
			事件玩家.WENzi = 自定义字符串("化影(主动):使自己隐身且无视透视,最多可持续12秒\r\n,移动会打断此效果(蹲下行走不会打断),冷却3秒\r\n{0}会晕眩敌人2.5秒\r\n{1}加速70%持续2秒\r\n{2}晕眩效果提升100%",
				技能图标字符串(英雄(麦克雷), 按钮(终极技能)), 技能图标字符串(英雄(麦克雷), 按钮(技能1)), 技能图标字符串(英雄(麦克雷), 按钮(技能2)));
		Else If(所用英雄(事件玩家) == 英雄(巴蒂斯特));
			事件玩家.WENzi = 自定义字符串(
