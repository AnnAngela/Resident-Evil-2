规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		全局.HUISHENG == 真;
		数组包含(范围内玩家(眼睛位置(选择英雄的玩家(英雄(回声), 所有队伍)), 12, 所有队伍, 表面), 事件玩家) == 真;
		数组包含(全局.b, 所用英雄(事件玩家)) == 真;
	}

	动作
	{
		设置状态(事件玩家, 选择英雄的玩家(英雄(回声), 所有队伍), 昏迷, 0.010);
		开始定向阈值(事件玩家, 方向(选择英雄的玩家(英雄(回声), 所有队伍), 事件玩家), 0.500, 至地图, 替换现有阈值, 无);
		小字体信息(选择英雄的玩家(英雄(回声), 所有队伍), 字符串("{0} {1}", 自定义字符串("已恐惧"), 所用英雄(事件玩家)));
		小字体信息(事件玩家, 自定义字符串("已被回声恐惧"));
		If(事件玩家 != 全局.P);
			等待(4, 无视条件);
		Else;
			等待(2, 无视条件);
		End;
		停止定向阈值(事件玩家);
		设置状态(事件玩家, 选择英雄的玩家(英雄(回声), 所有队伍), 昏迷, 1);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		巴蒂斯特;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
		在视线内(事件玩家, 距离准星最近的玩家(事件玩家, 所有队伍), 屏障不会阻挡视线) == 真;
		数组包含(全局.RL, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) != 真;
	}

	动作
	{
		伤害(事件玩家, 无, 生命值(事件玩家) * 0.400);
		传送(事件玩家, 距离准星最近的玩家(事件玩家, 所有队伍));
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 80);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 5);
		小字体信息(事件玩家, 自定义字符串("风颂进入冷却2秒"));
		等待(2, 无视条件);
		小字体信息(事件玩家, 自定义字符串("风颂冷却就绪,可再次释放"));
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		巴蒂斯特;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		正在使用技能 1(事件玩家) == 真;
	}

	动作
	{
		事件玩家.GONGYONG = (最大生命值(事件玩家) - 生命值(事件玩家)) / 3;
		等待(0.050, 无视条件);
		If(生命值(事件玩家) <= 最大生命值(事件玩家) / 4);
			等待(0.200, 无视条件);
			设置技能冷却(事件玩家, 按钮(技能1), 5);
		End;
		治疗(事件玩家, 事件玩家, 50 + 事件玩家.GONGYONG);
		小字体信息(事件玩家, 字符串("{0} {1} {2}", 技能图标字符串(英雄(巴蒂斯特), 按钮(技能1)), 自定义字符串("额外恢复生命值为"), 事件玩家.GONGYONG));
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		半藏;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件技能 != 按钮(近身攻击);
	}

	动作
	{
		If(事件技能 == 按钮(终极技能));
			伤害(被攻击方, 事件玩家, 2300);
		Else If(事件技能 == 按钮(技能1));
			伤害(被攻击方, 事件玩家, 350);
			设置状态(被攻击方, 事件玩家, 击倒, 2);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(半藏), 按钮(主要攻击模式)), 自定义字符串("致命一击！")));
		Else If(事件技能 == 按钮(技能2));
			播放效果(事件玩家, 有益选择效果 , 绿色, 事件玩家, 1);
			播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 50);
			治疗(事件玩家, 无, 30);
			设置状态(被攻击方, 事件玩家, 昏迷, 0.001);
			施加推力(被攻击方, 面朝方向(攻击方), 19, 至地图, 合并相反运动);
		End;
		设置技能冷却(事件玩家, 按钮(跳跃), 技能冷却时间(事件玩家, 按钮(跳跃)) - 2);
	}
}

