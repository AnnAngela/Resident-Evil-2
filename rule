	}

	动作
	{
		设置终极技能充能(事件玩家, 60);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		法老之鹰;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
		终极技能充能百分比(事件玩家) >= 30;
	}

	动作
	{
		设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) - 30);
		小字体信息(事件玩家, 自定义字符串("升级中···"));
		播放效果(事件玩家, 正面状态施加声音, 白色, 事件玩家, 150);
		等待(0.500, 无视条件);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		播放效果(事件玩家, 有害选择效果 , 蓝色, 事件玩家, 5);
		事件玩家.b += 1;
		事件玩家.GONGJI += 20;
		事件玩家.a += 20;
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		设置弹道速度(事件玩家, 事件玩家.a + 100);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		安娜;
	}

	条件
	{
		游戏正在进行中 == 真;
		比赛时间 <= 350;
		存活(事件玩家) == 真;
		事件玩家.XUANZHE == 1;
		事件玩家.GONGYONG == 5;
	}

	动作
	{
		设置造成伤害(事件玩家, 1);
		设置状态(事件玩家, 无, 击倒, 1);
		修改全局变量(a, 根据值从数组中移除, 所用英雄(事件玩家));
		大字体信息(所有玩家(所有队伍), 字符串("{0} {1} {2}", 英雄图标字符串(英雄(安娜)), 自定义字符串("加入了感染体阵营"), 技能图标字符串(英雄(莫伊拉), 按钮(主要攻击模式))));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		创建地图文本(所有玩家(所有队伍), 图标字符串(骷髅), 事件玩家, 1.500, 根据表面截取, 可见和位置, 紫色, 默认可见度);
		事件玩家.WENBEN = 上一个文本ID;
		等待(60, 当为“假”时中止);
		大字体信息(所有玩家(所有队伍), 字符串("{0} {1} {2}", 英雄图标字符串(英雄(安娜)), 自定义字符串("安全度过了虚弱阶段,变身为"), 技能图标字符串(英雄(莫伊拉), 按钮(技能1))));
		消除地图文本(事件玩家.WENBEN);
		创建图标(所有玩家(所有队伍), 事件玩家, 骷髅, 无, 紫色, 假);
		事件玩家.GONGYONG = 6;
		设置主要攻击模式启用(事件玩家, 假);
		设置造成伤害(事件玩家, 250);
		修改全局变量(TOUSHI, 添加至数组, 所用英雄(事件玩家));
	}
}

