规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		路霸;
	}

	条件
	{
		正在使用技能 2(事件玩家) == 真;
	}

	动作
	{
		If(事件玩家.b == 1);
			设置不可见(事件玩家, 全部);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(路霸), 按钮(技能2)), 自定义字符串("已进入隐身状态")));
			事件玩家.a = 2;
		Else If(事件玩家.b == 2 && 全局.SHIJIAN != 真);
			设置移动速度(事件玩家, 50);
			创建效果(所有玩家(所有队伍), 球体, 红色, 事件玩家, 7, 可见，位置和半径);
			事件玩家.XIAOG1 = 最后创建的实体;
