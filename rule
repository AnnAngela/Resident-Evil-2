		等待(6, 无视条件);
		小字体信息(事件玩家, 自定义字符串("{冲刺冷却完毕}"));
	}
}

规则("")
{
	事件
	{
		玩家受到伤害;
		双方;
		布丽吉塔;
	}

	动作
	{
		If(事件玩家.XUANZHE == 1);
			事件玩家.GONGYONG += 事件伤害 * 0.200;
		Else If(事件技能 == 按钮(技能1) || 事件技能 == 按钮(技能2));
			设置启用技能 2(事件玩家, 真);
			事件玩家.b = 0;
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		布丽吉塔;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		事件玩家.GONGYONG >= 100;
	}

	动作
	{
		事件玩家.GONGYONG = 0;
		事件玩家.GONGJI += 10;
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		事件玩家.SM += 5;
		设置最大生命值(事件玩家, 事件玩家.SM + 100);
		小字体信息(事件玩家, 自定义字符串("增加10%伤害和5%生命值"));
		事件玩家.b += 1;
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 150);
		治疗(事件玩家, 无, 300);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		布丽吉塔;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		伤害(范围内玩家(事件玩家, 10, 所有队伍, 关闭), 事件玩家, 450);
		设置状态(已过滤的数组(范围内玩家(事件玩家, 10, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 事件玩家, 击倒, 1.500);
		施加推力(已过滤的数组(范围内玩家(事件玩家, 10, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 地图矢量(矢量(0, 1, 1), 事件玩家, 旋转), 15, 至地图, 合并相反运动);
		播放效果(事件玩家, 正面状态施加声音, 白色, 事件玩家, 40);
		播放效果(所有玩家(所有队伍), 环状爆炸, 黄色, 事件玩家, 12);
		小字体信息(事件玩家, 自定义字符串("霸气进入冷却14秒"));
		等待(14, 无视条件);
		小字体信息(事件玩家, 自定义字符串("霸气冷却完毕，可再次使用"));
	}
}

