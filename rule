规则("")
{
	事件
	{
		子程序;
		Sub1;
	}

	动作
	{
		事件玩家.GONGYONG = 2;
		小字体信息(事件玩家, 自定义字符串("正在复活中···"));
		等待(1.500, 无视条件);
		重生(事件玩家);
		小字体信息(事件玩家, 自定义字符串("断尾进入冷却6秒"));
		事件玩家.a = 0;
		等待(0.100, 无视条件);
		事件玩家.SM -= 15;
		设置最大生命值(事件玩家, 事件玩家.SM + 100);
		等待(5.900, 无视条件);
		小字体信息(事件玩家, 自定义字符串("断尾冷却完毕,可再次释放"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		事件玩家.GONGYONG = 0;
	}
}

规则("")
{
	事件
	{
		玩家阵亡;
		双方;
		安娜;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件玩家.a != 1;
	}

	动作
	{
		等待(1.500, 当为“假”时中止);
		If(事件玩家.b == 0);
			小字体信息(事件玩家, 自定义字符串("正在使用魂归,复活中···"));
			等待(3.500, 无视条件);
			重生(事件玩家);
			等待(1.500, 无视条件);
			小字体信息(事件玩家, 自定义字符串("魂归技能已失效"));
			事件玩家.SM = 0;
			设置最大生命值(事件玩家, 100);
			事件玩家.b = 1;
		Else;
			设置最大重生时间(事件玩家, 8);
			消除地图文本(事件玩家.TOUSHI);
			调用子程序(Sub);
		End;
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		安娜;
	}

	条件
	{
		事件玩家.XUANZHE >= 2;
		事件技能 == 按钮(技能2);
	}

	动作
	{
		伤害(被攻击方, 事件玩家, 150);
		If(被攻击方 == 全局.P);
			设置状态(被攻击方, 事件玩家, 击倒, 1);
		Else;
			设置状态(被攻击方, 事件玩家, 击倒, 2);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		艾什;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		终极技能充能百分比(事件玩家) >= 20;
		按钮被按下(事件玩家, 互动) == 真;
		事件玩家.GONGYONG != 15;
	}

	动作
	{
		事件玩家.GONGYONG = 随机整数(1, 7);
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		设置移动速度(事件玩家, 事件玩家.SH + 100);
		设置受到伤害(事件玩家, 100);
		小字体信息(事件玩家, 自定义字符串("正在抽取道具"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 75);
		等待(0.250, 无视条件);
		If(事件玩家.GONGYONG == 1);
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到道具防护服"));
			事件玩家.GONGYONG = 13;
			事件玩家.b = 自定义字符串("防护服:受到伤害永久减少30%\r\n使用后效果提升至80%持续5秒.之后会消失");
			设置受到伤害(事件玩家, 70);
		Else If(事件玩家.GONGYONG == 2);
			事件玩家.GONGYONG = 8;
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到精制武器"));
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 150);
			事件玩家.b = 自定义字符串("精制武器:永久提升50%伤害\r\n使用后效果提升至200%持续5秒.之后会消失");
		Else If(事件玩家.GONGYONG == 3);
			事件玩家.GONGYONG = 9;
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到优制皮鞋"));
			设置移动速度(事件玩家, 事件玩家.SH + 125);
			事件玩家.b = 自定义字符串("优制皮鞋:永久提升25%速度\r\n使用后效果提升至100%持续4秒.之后会消失");
		Else If(事件玩家.GONGYONG == 4);
			事件玩家.GONGYONG = 10;
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到护身符"));
			事件玩家.b = 自定义字符串("护身符:随机传送到安全位置\r\n使用后消失");
		Else If(事件玩家.GONGYONG == 5);
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到道具高级餐点"));
			事件玩家.GONGYONG = 11;
			事件玩家.b = 自定义字符串("高级餐点\r\n使用后立马恢复满生命值并消散3秒");
		Else If(事件玩家.GONGYONG == 6);
			事件玩家.GONGYONG = 12;
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到优质饼干"));
			事件玩家.b = 自定义字符串("优质饼干\r\n使用后永久提升10%攻击力和10%生命值\r\n并增加10%能量");
		Else;
			事件玩家.c += 1;
			小字体信息(事件玩家, 自定义字符串("恭喜你抽到一块未知碎片"));
			事件玩家.b = 自定义字符串("未知碎片  {0}/3\r\n似乎收集三个才能拼凑完整？？？\r\n收集三个后才可使用", 事件玩家.c);
			事件玩家.GONGYONG = 14;
		End;
		设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) - 20);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 150);
		等待(0.500, 无视条件);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		艾什;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		事件玩家.GONGYONG > 7;
		按钮被按下(事件玩家, 终极技能) == 真;
		具有状态(事件玩家, 被入侵) != 真;
	}

	动作
	{
		If(事件玩家.GONGYONG == 13);
			事件玩家.GONGYONG = 15;
			设置受到伤害(事件玩家, 20);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 3);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("你已触发防护服效果,伤害减免85%持续4秒"));
			事件玩家.b = 自定义字符串("正在使用防护服中···");
			等待(5, 无视条件);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("防护服已破碎"));
			设置受到伤害(事件玩家, 100);
		Else If(事件玩家.GONGYONG == 8);
			事件玩家.GONGYONG = 15;
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 300);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 3);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("你已触发精制武器效果,伤害提高200%持续5秒"));
			事件玩家.b = 自定义字符串("正在使用精制武器中···");
			等待(5, 无视条件);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("精制武器已破碎"));
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		Else If(事件玩家.GONGYONG == 9);
			事件玩家.GONGYONG = 15;
			设置移动速度(事件玩家, 事件玩家.SH + 200);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 3);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("你已触发精制皮鞋效果,速度提高100%持续4秒"));
			事件玩家.b = 自定义字符串("正在使用精制皮鞋中···");
			等待(4, 无视条件);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("精制皮鞋已破碎"));
			设置移动速度(事件玩家, 事件玩家.SH + 100);
		Else If(事件玩家.GONGYONG == 10);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 3);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("你已触发护身符效果,已转移至安全地区"));
			重生(事件玩家);
		Else If(事件玩家.GONGYONG == 11);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 3);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("你已触发高级糕点效果,获得无敌效果3秒"));
			设置状态(事件玩家, 无, 消散, 3);
			治疗(事件玩家, 无, 2000);
		Else If(事件玩家.GONGYONG == 12);
			事件玩家.SM += 10;
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 绿色, 事件玩家, 3);
			播放效果(事件玩家, 状态爆炸声音, 绿色, 事件玩家, 100);
			小字体信息(事件玩家, 自定义字符串("你已使用优质饼干"));
			事件玩家.GONGJI += 10;
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
			设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) + 10);
		Else If(事件玩家.GONGYONG == 14 && 事件玩家.c >= 3);
			大字体信息(所有玩家(所有队伍), 自定义字符串("某种神秘力量席卷了艾什,艾什被传送离开了此地"));
			修改玩家分数(事件玩家, 事件玩家 + 22);
			调用子程序(Sub0);
		End;
		事件玩家.b = 自定义字符串("你没有道具");
		事件玩家.GONGYONG = 0;
		等待(0.250, 无视条件);
	}
}

