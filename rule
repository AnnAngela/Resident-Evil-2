			等待(10, 无视条件);
			设置造成伤害(事件玩家, 100);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(路霸), 按钮(技能2)), 自定义字符串("伤害提升已失效")));
		Else If(事件玩家.b == 4 && 全局.SHIJIAN != 真);
			事件玩家.a = 80;
			设置移动速度(事件玩家, 125);
			播放效果(所有玩家(所有队伍), 爆炸声音, 白色, 事件玩家, 100);
			播放效果(所有玩家(所有队伍), 有害选择效果 , 红色, 事件玩家, 4);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(路霸), 按钮(技能2)), 自定义字符串("速度提升25%")));
			设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) + 25);
			等待(5, 无视条件);
			事件玩家.a = 0;
			设置移动速度(事件玩家, 100);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(路霸), 按钮(技能2)), 自定义字符串("不可控已失效")));
		End;
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		路霸;
	}

	动作
	{
		If(事件玩家.b == 1 && 事件玩家.a == 2);
			伤害(被攻击方, 事件玩家, 40);
			设置状态(事件玩家, 无, 昏迷, 1.300);
			设置不可见(事件玩家, 无);
			事件玩家.a = 0;
		End;
		If(事件技能 == 按钮(技能1));
			If(事件玩家.b == 3);
				等待(0.100, 无视条件);
				设置技能冷却(事件玩家, 按钮(技能1), 技能冷却时间(事件玩家, 按钮(技能1)) / 4);
			Else If(事件玩家.b == 5);
				等待(0.050, 无视条件);
				传送(事件玩家, 眼睛位置(被攻击方));
				设置状态(事件玩家, 无, 无敌, 4);
			Else If(事件玩家.b == 6);
				等待(0.010, 无视条件);
				设置状态(事件玩家, 无, 昏迷, 0.001);
				被攻击方.SH -= 60;
				设置移动速度(被攻击方, 被攻击方.SH + 100);
				等待(1.500, 无视条件);
				被攻击方.SH += 60;
				设置移动速度(被攻击方, 被攻击方.SH + 100);
			End;
		End;
	}
}

