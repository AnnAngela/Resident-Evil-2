		小字体信息(事件玩家, 自定义字符串("已选择完毕,请善用你的技能"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 75);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		游戏正在进行中 == 真;
		数组包含(从数组中移除(全局.a, 英雄(托比昂)), 所用英雄(事件玩家)) == 真;
		事件玩家.XUANZHE == 0;
		按钮被按下(事件玩家, 辅助攻击模式) == 真;
		按钮被按下(事件玩家, 主要攻击模式) != 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("正在选择职业中···②"));
		等待(1.500, 当为“假”时中止);
		事件玩家.XUANZHE = 2;
		设置主要攻击模式启用(事件玩家, 真);
		设置辅助攻击模式启用(事件玩家, 真);
		If(所用英雄(事件玩家) == 英雄(秩序之光));
			事件玩家.a = 1;
			设置主要攻击模式启用(事件玩家, 假);
			设置弹道速度(事件玩家, 0);
			全局.F = 随机整数(1, 25);
			事件玩家.WENzi = 自定义字符串(
				"阵法·炸(主动):(按住蹲和互动使用)布置一个陷阱\r\n敌人踩上会晕眩5秒并造成大量伤害(只能存在一个)\r\n阵法·护(被动):右键命中敌人会晕眩其0.35秒\r\n希望之盒(被动):花费25%能量按下(shift)使用\r\n从1~25数字里面输入一个数字,输对则逃");
		Else If(所用英雄(事件玩家) == 英雄(天使));
			事件玩家.WENzi = 自定义字符串("{0}向前方飞行,并加速3秒.冷却16秒\r\n{1}使用法杖右键会使自己停在空\r\n中(最多可持续7秒),停止使用后进入冷却6秒\r\n{2}使自己获得无敌状态持续3秒", 技能图标字符串(英雄(天使), 按钮(
				技能1)), 技能图标字符串(英雄(天使), 按钮(主要攻击模式)), 技能图标字符串(英雄(天使), 按钮(技能2)));
		Else If(所用英雄(事件玩家) == 英雄(美));
			事件玩家.c = 12;
			事件玩家.b = 3;
			事件玩家.WENzi = 自定义字符串(
				"冰封(主动):按下(F/互动)使用建造一个区域\r\n在3秒后冻结范围内感染体,冷却16秒\r\n{0}冷却降低至1.6秒\r\n{1}结束后额外恢复100点生命并加速\r\n凝寒(任务):每冻结一名敌人,会使冰封范围\r\n半径扩大0.3米.冻结延长0.1秒",
				技能图标字符串(英雄(美), 按钮(技能2)), 技能图标字符串(英雄(美), 按钮(技能1)));
		Else If(所用英雄(事件玩家) == 英雄(回声));
			设置终极技能充能(事件玩家, 20);
			事件玩家.WENzi = 自定义字符串("恐惧(主动):立即恐惧周围感染体,使他们不断缓慢后退\r\n持续4秒(母体为2秒),结束后晕眩1秒.冷却16秒\r\n{0}只能复制人类,复制感染体会死亡", 技能图标字符串(英雄(回声), 按钮(终极技能)));
		Else If(所用英雄(事件玩家) == 英雄(麦克雷));
			创建地图文本(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TOUSHI, 所用英雄(当前数组元素))), 自定义字符串("存活中"), 事件玩家, 1.500, 不要截取, 可见，位置和字符串, 青绿色, 默认可见度);
			事件玩家.TOUSHI = 上一个文本ID;
			事件玩家.WENzi = 自定义字符串("{0}期间所有感染体无法移动\r\n蓄力满五秒后会秒杀一切感染体\r\n{1}恢复30+35%已损失生命\r\n增加30%伤害持续4秒,重复翻滚\r\n可叠加并刷新持续时间\r\n{2}命中感染体减少翻滚1.2秒冷却",
				技能图标字符串(英雄(麦克雷), 按钮(终极技能)), 技能图标字符串(英雄(麦克雷), 按钮(技能1)), 技能图标字符串(英雄(麦克雷), 按钮(主要攻击模式)));
		Else If(所用英雄(事件玩家) == 英雄(巴蒂斯特));
			事件玩家.SM += 50;
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
			小字体信息(事件玩家, 自定义字符串("生命值提高了50%"));
			事件玩家.WENzi = 自定义字符串(
				"共生(主动):近距离选择一个人类目标,双方彼此获\r\n得10%速度.对象死亡你会有惩罚(仅可用一次)\r\n相守(主动):(选择目标后可用)会召唤对象,为他恢\r\n复100生命,并为其刷新技能CD,冷却16秒\r\n传送门(被动):大招会开启一个7秒传送门");
		Else If(所用英雄(事件玩家) == 英雄(卢西奥));
			事件玩家.SM -= 30;
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
