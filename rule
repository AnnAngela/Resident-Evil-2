规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		游戏正在进行中 == 真;
		比赛时间 < 360;
		数组包含(范围内玩家(全局.Y, 3, 所有队伍, 关闭), 事件玩家) == 真;
		数组包含(全局.a, 所用英雄(事件玩家)) == 真;
		存活(事件玩家) == 真;
		具有状态(事件玩家, 被入侵) != 真;
		全局.TSD == 真;
	}

	动作
	{
		If(所用英雄(事件玩家) == 英雄(托比昂));
			If(选择英雄的玩家(英雄(天使), 所有队伍) == 全局.U);
				修改玩家分数(选择英雄的玩家(英雄(安娜), 所有队伍), 选择英雄的玩家(英雄(安娜), 所有队伍) + 25);
				修改全局变量(b, 添加至数组, 英雄(安娜));
				修改全局变量(a, 根据值从数组中移除, 英雄(安娜));
			End;
			设置状态(选择英雄的玩家(英雄(天使), 所有队伍), 无, 被入侵, 9999);
			传送(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TIANSHI, 所用英雄(当前数组元素))), 全局.TL);
			等待(0.050, 无视条件);
			修改全局变量(b, 添加至数组, 英雄(天使));
			修改全局变量(a, 根据值从数组中移除, 英雄(天使));
			修改全局变量(RL, 添加至数组, 选择英雄的玩家(英雄(天使), 所有队伍));
			全局.TAOWANG += 英雄数量(英雄(托比昂), 所有队伍) + 1;
			设置受到伤害(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TIANSHI, 所用英雄(当前数组元素))), 0);
			设置状态(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TIANSHI, 所用英雄(当前数组元素))), 无, 被入侵, 9999);
			大字体信息(所有玩家(所有队伍), 自定义字符串("托比与天使已逃离"));
			修改玩家分数(选择英雄的玩家(英雄(天使), 所有队伍), 选择英雄的玩家(英雄(天使), 所有队伍) + 10);
			修改玩家分数(选择英雄的玩家(英雄(托比昂), 所有队伍), 选择英雄的玩家(英雄(托比昂), 所有队伍) + 10);
		Else;
			修改玩家分数(事件玩家, 事件玩家 + 10);
		End;
		调用子程序(Sub0);
	}
}

