规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		猎空;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		存活(事件玩家) == 真;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		设置引力(事件玩家, 0);
		小字体信息(事件玩家, 自定义字符串("引力已清零,持续5秒"));
		施加推力(事件玩家, 矢量(0, 500, 0), 7, 至地图, 取消相反运动);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 天蓝色, 事件玩家, 3);
		等待(5, 无视条件);
		设置引力(事件玩家, 60);
		小字体信息(事件玩家, 自定义字符串("失重进入冷却12秒"));
		等待(12, 无视条件);
		小字体信息(事件玩家, 自定义字符串("失重冷却就绪,可再次使用"));
	}
}

