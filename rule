规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		末日铁拳;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		数组包含(全局.b, 所用英雄(被攻击方)) == 真;
	}

	动作
	{
		设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) + 5);
		设置技能冷却(事件玩家, 按钮(技能1), 技能冷却时间(事件玩家, 按钮(技能1)) - 1.400);
		设置技能冷却(事件玩家, 按钮(技能2), 技能冷却时间(事件玩家, 按钮(技能2)) - 1.400);
		设置技能冷却(事件玩家, 按钮(辅助攻击模式), 技能冷却时间(事件玩家, 按钮(辅助攻击模式)) - 1.400);
		治疗(事件玩家, 事件玩家, 30);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		黑影;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		数组包含(全局.a, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) == 真;
		在视线内(眼睛位置(事件玩家), 眼睛位置(距离准星最近的玩家(事件玩家, 所有队伍)), 屏障不会阻挡视线) == 真;
		数组包含(全局.RL, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) != 真;
		正在使用辅助武器(事件玩家) == 真;
	}

	动作
	{
		设置技能冷却(事件玩家, 按钮(辅助攻击模式), 10);
		传送(距离准星最近的玩家(事件玩家, 所有队伍), 事件玩家);
		取消主要动作(事件玩家);
		If(生命值(距离准星最近的玩家(事件玩家, 所有队伍)) <= 最大生命值(事件玩家) * 0.500);
			修改玩家分数(事件玩家, 事件玩家 + 2);
			小字体信息(事件玩家, 自定义字符串("治疗低生命值人类,分数+2,能量+10%"));
			设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) + 10);
		End;
		小字体信息(事件玩家, 自定义字符串("已将{0}转移至自己身边", 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))));
		设置状态(距离准星最近的玩家(事件玩家, 所有队伍), 事件玩家, 定身, 0.500);
		等待(0.050, 无视条件);
		治疗(距离准星最近的玩家(事件玩家, 所有队伍), 事件玩家, 250);
		小字体信息(距离准星最近的玩家(事件玩家, 所有队伍), 自定义字符串("{0}将你转移至身边并治疗了你", 英雄图标字符串(英雄(黑影))));
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		正在使用终极技能(选择英雄的玩家(英雄(黑影), 所有队伍)) == 真;
	}

	动作
	{
		If(选择英雄的玩家(英雄(黑影), 所有队伍).XUANZHE == 2);
			治疗(已过滤的数组(所有存活玩家(所有队伍), 数组包含(全局.a, 所用英雄(当前数组元素))), 选择英雄的玩家(英雄(黑影), 所有队伍), 1000);
			设置状态(已过滤的数组(范围内玩家(选择英雄的玩家(英雄(黑影), 所有队伍), 15, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 选择英雄的玩家(英雄(黑影), 所有队伍), 击倒, 3);
			小字体信息(所有玩家(所有队伍), 字符串("{0} {1}", 技能图标字符串(英雄(黑影), 按钮(终极技能)), 自定义字符串("所有人类回满生命")));
		Else If(全局.HEIY >= 20);
			击杀(已过滤的数组(范围内玩家(眼睛位置(选择英雄的玩家(英雄(黑影), 所有队伍)), 15, 所有队伍, 表面及敌方屏障), 数组包含(全局.b, 所用英雄(当前数组元素))), 选择英雄的玩家(英雄(黑影), 所有队伍));
		Else;
			伤害(范围内玩家(选择英雄的玩家(英雄(黑影), 所有队伍), 15, 所有队伍, 关闭), 选择英雄的玩家(英雄(黑影), 所有队伍), 1475);
			设置技能冷却(选择英雄的玩家(英雄(黑影), 所有队伍), 按钮(辅助攻击模式), 5);
		End;
		等待(6, 无视条件);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		麦克雷;
	}

	条件
	{
		正在使用技能 1(事件玩家) == 真;
	}

	动作
	{
		If(事件玩家.XUANZHE == 2);
			治疗(事件玩家, 无, (最大生命值(事件玩家) - 生命值(事件玩家)) * 0.350 + 30);
			播放效果(所有玩家(所有队伍), 有益爆炸, 绿色, 事件玩家, 1);
			事件玩家.a += 30;
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 事件玩家.a + 30);
			小字体信息(事件玩家, 自定义字符串("当前增加伤害为{0}%", 事件玩家.a));
			等待(4, 当为“真”时重新开始);
			事件玩家.a = 0;
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		Else If(事件玩家.XUANZHE == 1);
			设置移动速度(事件玩家, 事件玩家.SH + 170);
			播放效果(所有玩家(所有队伍), 负面状态施加声音, 白色, 事件玩家, 90);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(麦克雷), 按钮(技能1)), 自定义字符串("加速70%持续2秒")));
			等待(2, 无视条件);
			设置移动速度(事件玩家, 事件玩家.SH + 100);
		End;
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		麦克雷;
	}

	条件
	{
		事件技能 != 按钮(辅助攻击模式);
	}

	动作
	{
		If(事件玩家.XUANZHE == 1);
			If(事件技能 == 按钮(技能2));
				设置状态(被攻击方, 事件玩家, 昏迷, 1.700);
			Else If(事件技能 == 按钮(终极技能));
				设置状态(被攻击方, 事件玩家, 昏迷, 2.500);
			End;
		Else If(事件技能 == 按钮(主要攻击模式));
			设置技能冷却(事件玩家, 按钮(技能1), 技能冷却时间(事件玩家, 按钮(技能1)) - 1.200);
		End;
	}
}

