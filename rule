规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		莫伊拉;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		全局.E = 射线命中位置(眼睛位置(事件玩家), 眼睛位置(事件玩家) + 面朝方向(事件玩家) * 18, 无, 无, 假);
		等待(0.050, 无视条件);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 紫色, 全局.E, 5);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 100);
		等待(0.100, 无视条件);
		创建效果(所有玩家(所有队伍), 云, 紫色, 全局.E, 8, 可见，位置和半径);
		事件玩家.d = 最后创建的实体;
		小字体信息(事件玩家, 自定义字符串("已释放毒雾,持续10秒,技能进入冷却25秒"));
		创建效果(所有玩家(所有队伍), 有害光环, 紫色, 全局.E, 8, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		全局.moyi = 真;
		等待(10, 无视条件);
		全局.moyi = 假;
		消除效果(事件玩家.d);
		消除效果(事件玩家.XIAOG1);
		全局.E = 矢量(0, -500, 0);
		等待(25, 无视条件);
		小字体信息(事件玩家, 自定义字符串("毒雾冷却就绪,可再次释放"));
		播放效果(事件玩家, 状态爆炸声音, 白色, 事件玩家, 100);
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		选择英雄的玩家(英雄(莫伊拉), 所有队伍).XUANZHE == 2;
		全局.moyi == 真;
	}

	动作
	{
		While(选择英雄的玩家(英雄(莫伊拉), 所有队伍).XUANZHE == 2 && 全局.moyi == 真);
			伤害(已过滤的数组(范围内玩家(全局.E, 8, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 选择英雄的玩家(英雄(莫伊拉), 所有队伍), 150);
			治疗(已过滤的数组(范围内玩家(全局.E, 8, 所有队伍, 关闭), 数组包含(全局.a, 所用英雄(当前数组元素))), 选择英雄的玩家(英雄(莫伊拉), 所有队伍), 30);
			等待(1, 无视条件);
		End;
	}
}

