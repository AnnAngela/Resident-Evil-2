	{
		小字体信息(事件玩家, 自定义字符串("飞行技能已发动"));
		播放效果(所有玩家(所有队伍), 正面状态施加声音, 白色, 事件玩家, 80);
		播放效果(所有玩家(所有队伍), 环状爆炸, 蓝色, 事件玩家, 2);
		施加推力(事件玩家, 地图矢量(矢量(0, 1, 1), 事件玩家, 旋转), 27, 至地图, 取消相反运动);
		设置技能冷却(事件玩家, 按钮(技能1), 16);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		天使;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		正在使用辅助武器(事件玩家) == 真;
	}

	动作
	{
		事件玩家.W = 真;
		设置引力(事件玩家, 0);
		施加推力(事件玩家, 上, 0.001, 至地图, 取消相反运动);
		施加推力(事件玩家, 速率(事件玩家) * -1, 0.001, 至地图, 取消相反运动);
		等待(7, 当为“假”时中止);
		设置辅助攻击模式启用(事件玩家, 假);
		小字体信息(事件玩家, 自定义字符串("你悬浮太久了"));
		等待(6, 无视条件);
		设置辅助攻击模式启用(事件玩家, 真);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		天使;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		正在使用辅助武器(事件玩家) != 真;
		事件玩家.W == 真;
	}

	动作
	{
		事件玩家.W = 假;
		设置辅助攻击模式启用(事件玩家, 假);
		小字体信息(事件玩家, 自定义字符串("{你已停止使用悬浮，进入冷却6秒}"));
		设置引力(事件玩家, 100);
		等待(6, 无视条件);
		小字体信息(事件玩家, 自定义字符串("{悬浮可再次使用}"));
		设置辅助攻击模式启用(事件玩家, 真);
	}
}

规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		天使;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		数组包含(添加至数组(全局.S, 全局.D), 所用英雄(被攻击方)) == 真;
		被攻击方 != 全局.N;
	}

	动作
	{
		If(事件玩家.GONGYONG != 1);
			创建地图文本(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TIANSHI, 所用英雄(当前数组元素))), 字符串("{0} {1}", 英雄图标字符串(英雄(天使)), 取整(生命值(事件玩家), 至最近)), 事件玩家, 1.500, 不要截取,
				可见，位置和字符串, 黄色, 默认可见度);
			事件玩家.WENBEN = 上一个文本ID;
			事件玩家.GONGYONG = 1;
		End;
		被攻击方.WENzi = 自定义字符串(
			"转化者(被动):无视母体透视\r\n契约(被动):天使死亡你也会死亡\r\n一体(被动):其中一个托比昂逃离后\r\n会使所有托比昂和天使逃离\r\n重击(被动):锤子伤害会晕眩敌人0.75秒\r\n(母体仅0.3秒)冷却1秒");
		If(所用英雄(被攻击方) == 英雄(路霸));
			修改全局变量(bianshen, 添加至数组, 被攻击方.b);
		Else;
			修改全局变量(bianshen1, 添加至数组, 被攻击方.b);
		End;
		事件玩家.SM -= 15;
		设置最大生命值(事件玩家, 事件玩家.SM + 100);
		开始强制玩家选择英雄(被攻击方, 英雄(托比昂));
		修改玩家分数(事件玩家, 事件玩家 + 2);
		设置主要攻击模式启用(被攻击方, 真);
		设置辅助攻击模式启用(被攻击方, 真);
		大字体信息(被攻击方, 自定义字符串("{你已被天使奴役}"));
		设置启用技能 1(被攻击方, 假);
		小字体信息(事件玩家, 自定义字符串("奴役成功,分数+2奴役术进入冷却40秒"));
		等待(1, 无视条件);
		小字体信息(被攻击方, 自定义字符串("天使死亡你也会死亡"));
		创建地图文本(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.TIANSHI, 所用英雄(当前数组元素))), 字符串("{0} {1}", 英雄图标字符串(英雄(托比昂)), 取整(生命值(被攻击方), 至最近)), 被攻击方, 1.500, 不要截取,
			可见，位置和字符串, 黄色, 默认可见度);
		被攻击方.WENBEN = 上一个文本ID;
		消除效果(被攻击方.XIAOG1);
		设置不可见(被攻击方, 无);
		设置弹道速度(被攻击方, 100);
		设置引力(被攻击方, 100);
		设置启用技能 2(被攻击方, 真);
		设置最大生命值(被攻击方, 100);
		设置造成伤害(被攻击方, 100);
		可用按钮(被攻击方, 终极技能);
		设置移动速度(被攻击方, 100);
		等待(40, 无视条件);
		小字体信息(事件玩家, 自定义字符串("奴役术技能冷却完毕"));
		播放效果(事件玩家, 状态爆炸声音, 白色, 事件玩家, 100);
	}
}

规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		源氏;
	}

	条件
	{
		数组包含(全局.b, 所用英雄(被攻击方)) == 真;
	}

	动作
	{
		If(事件玩家.XUANZHE == 1);
			设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) + 5);
			小字体信息(事件玩家, 自定义字符串("已为自己恢复20生命"));
			治疗(事件玩家, 事件玩家, 20);
		Else If(事件玩家.XUANZHE == 2);
			设置状态(事件玩家, 无, 消散, 0.500);
			设置移动速度(事件玩家, 1000);
			开始定向阈值(事件玩家, 地图矢量(矢量(0, 0, 1), 事件玩家, 旋转), 1, 至地图, 替换现有阈值, 方向和幅度);
			等待(0.050, 无视条件);
			停止定向阈值(事件玩家);
			设置移动速度(事件玩家, 100);
			设置技能冷却(事件玩家, 按钮(技能1), 0);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		源氏;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		播放效果(所有玩家(所有队伍), 环状爆炸, 红色, 事件玩家, 10);
		播放效果(所有玩家(所有队伍), 有害爆炸, 红色, 事件玩家, 6);
		播放效果(所有玩家(所有队伍), 正面状态施加声音, 白色, 事件玩家, 100);
		设置朝向(已过滤的数组(范围内玩家(事件玩家, 12, 所有队伍, 关闭), 数组包含(全局.b, 所用英雄(当前数组元素))), 矢量(0, 500, 0), 至地图);
		小字体信息(事件玩家, 自定义字符串("筑梦进入冷却12秒"));
		等待(12, 无视条件);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		小字体信息(事件玩家, 自定义字符串("筑梦冷却完毕,可再次释放"));
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		源氏;
	}

	条件
	{
		(事件技能 == 按钮(技能1) || 事件技能 == 按钮(终极技能)) == 真;
		所用英雄(被攻击方) != 英雄(堡垒);
	}

	动作
	{
		If(事件玩家.XUANZHE == 2);
			事件玩家.GONGJI += 4;
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
		End;
		If(事件玩家.XUANZHE == 1);
			If(事件技能 == 按钮(技能1));
				If(生命值(被攻击方) > 最大生命值(被攻击方) / 2);
					设置状态(被攻击方, 事件玩家, 昏迷, 1);
					治疗(事件玩家, 事件玩家, 40);
				Else;
					设置状态(被攻击方, 事件玩家, 昏迷, 2);
					治疗(事件玩家, 事件玩家, 80);
				End;
			End;
		End;
		If(事件玩家.GONGYONG == 6);
			等待(0.400, 无视条件);
			设置技能冷却(事件玩家, 按钮(技能1), 0);
		End;
		If(事件玩家.XUANZHE == 3);
			治疗(事件玩家, 无, (最大生命值(事件玩家) - 生命值(事件玩家)) * 0.100);
			伤害(范围内玩家(被攻击方, 5, 所有队伍, 关闭), 事件玩家, 45);
			播放效果(所有玩家(所有队伍), 环状爆炸, 白色, 被攻击方, 5);
		End;
	}
}

