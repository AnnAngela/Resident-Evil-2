规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		猎空;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 装填) == 真;
		事件玩家.a > 0;
	}

	动作
	{
		等待(1.500, 当为“假”时中止);
		If(比赛时间 < 365);
			设置比赛时间(比赛时间 - 事件玩家.a);
			消除地图文本(事件玩家.WENBEN);
			事件玩家.b = 5;
			大字体信息(所有玩家(所有队伍), 自定义字符串("{0}已将比赛时间缩短{1}秒", 英雄图标字符串(英雄(猎空)), 事件玩家.a));
			修改玩家分数(事件玩家, 事件玩家 + 事件玩家.a * 0.100);
			设置慢动作(25);
			等待(0.250, 无视条件);
			事件玩家.a = 0;
			设置慢动作(100);
			消除效果(事件玩家.XIAOG1);
		Else;
			小字体信息(事件玩家, 自定义字符串("太早了，别那么快用"));
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		猎空;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		数组包含(范围内玩家(全局.H, 3, 所有队伍, 关闭), 事件玩家) == 真;
		事件玩家.b != 5;
	}

	动作
	{
		事件玩家.a += 10;
		小字体信息(事件玩家, 自定义字符串("能量提升了10%,能量球5秒后刷新"));
		播放效果(事件玩家, 状态爆炸声音, 白色, 事件玩家, 100);
		全局.H = 矢量(0, 1000, 0);
		等待(5, 无视条件);
		全局.H = 眼睛位置(数组随机取值(已过滤的数组(从数组中移除(所有存活玩家(所有队伍), 所用英雄(事件玩家)), 在地面上(当前数组元素) && 数组包含(全局.RL, 所用英雄(当前数组元素)) != 真)));
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
