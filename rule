规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		游戏正在进行中 == 真;
		事件玩家.XUANZHE == 0;
		比赛时间 > 380;
		事件玩家.E == 0;
		按钮被按下(事件玩家, 互动) == 真;
		(按钮被按下(事件玩家, 主要攻击模式) || 按钮被按下(事件玩家, 蹲下) || 按钮被按下(事件玩家, 辅助攻击模式)) != 真;
		数组包含(从数组中移除(全局.a, 英雄(托比昂)), 所用英雄(事件玩家)) == 真;
		分数(事件玩家) >= 5;
	}

	动作
	{
		事件玩家.E = 数组随机取值(全局.RM);
		修改全局变量(RM, 根据值从数组中移除, 事件玩家.E);
		小字体信息(事件玩家, 自定义字符串("已为你随机更换英雄"));
		修改玩家分数(事件玩家, 事件玩家 - 5);
		If(所用英雄(事件玩家) == 英雄(秩序之光));
			设置启用技能 1(事件玩家, 真);
		Else If(所用英雄(事件玩家) == 英雄(死神));
			设置启用技能 2(事件玩家, 真);
		Else If(所用英雄(事件玩家) == 英雄(卢西奥));
			设置造成治疗(事件玩家, 100);
		End;
		设置受到伤害(事件玩家, 100);
		清除状态(事件玩家, 无法杀死);
		等待(0.100, 无视条件);
		开始强制玩家选择英雄(事件玩家, 事件玩家.E);
	}
}

