规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		天使;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 技能1) == 真;
		技能冷却时间(事件玩家, 按钮(技能1)) == 0;
		所用英雄(距离准星最近的玩家(事件玩家, 所有队伍)) == 英雄(托比昂);
	}

	动作
	{
		事件玩家.a = 所选位置(事件玩家);
		事件玩家.b = 距离准星最近的玩家(事件玩家, 所有队伍);
		传送(事件玩家, 事件玩家.b);
		播放效果(所有玩家(所有队伍), 有益选择效果 , 紫色, 事件玩家, 3);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		等待(0.100, 无视条件);
		传送(事件玩家.b, 事件玩家.a);
		治疗(事件玩家.b, 事件玩家, 1000);
		设置状态(事件玩家.b, 无, 无敌, 2);
		设置技能冷却(事件玩家, 按钮(技能1), 6);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		回声;
	}

	条件
	{
		事件玩家.XUANZHE == 3;
		事件玩家.a != 1;
		存活(事件玩家) == 真;
		按钮被按下(事件玩家, 互动) == 真;
		在视线内(事件玩家, 距离准星最近的玩家(事件玩家, 所有队伍), 屏障不会阻挡视线) == 真;
		数组包含(全局.a, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) == 真;
	}

	动作
	{
		全局.O = 距离准星最近的玩家(事件玩家, 所有队伍);
		等待(0.100, 无视条件);
		绑定玩家(事件玩家, 全局.O, 矢量(0, 1.200, 0));
		播放效果(所有玩家(所有队伍), 有害选择效果 , 紫色, 事件玩家, 4);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 紫色, 事件玩家, 100);
		事件玩家.a = 1;
		小字体信息(事件玩家, 自定义字符串("已附着{0}", 所用英雄(全局.O)));
	}
}

