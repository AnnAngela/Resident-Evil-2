规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		死神;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		具有状态(事件玩家, 无法杀死) == 真;
		生命值(事件玩家) <= 1;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("触发命数未尽,持续4秒"));
		等待(4, 无视条件);
		清除状态(事件玩家, 无法杀死);
		小字体信息(事件玩家, 自定义字符串("命数未尽已失效,进入冷却16秒"));
		等待(16, 无视条件);
		If(所用英雄(事件玩家) == 英雄(死神));
			设置状态(事件玩家, 无, 无法杀死, 400);
			小字体信息(事件玩家, 自定义字符串("命数未尽冷却完毕,可再次触发"));
		End;
	}
}

