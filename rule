	动作
	{
		设置朝向(事件玩家, 矢量(0, -100, 0), 至地图);
		等待(0.016, 无视条件);
		设置状态(事件玩家, 无, 昏迷, 2);
		全局.J = 所选位置(事件玩家);
		全局.I = 数组随机取值(已过滤的数组(从数组中移除(所有玩家(所有队伍), 事件玩家), 数组包含(全局.RL, 所用英雄(当前数组元素)) != 真));
		等待(0.250, 无视条件);
		小字体信息(全局.I, 自定义字符串("狂鼠正在召唤你"));
		创建地图文本(所有玩家(所有队伍), 字符串("{0} {1}", 所用英雄(全局.I), 自定义字符串("正在到来")), 全局.J, 2, 根据表面截取, 可见，位置和字符串, 橙色, 默认可见度);
		事件玩家.WENBEN = 上一个文本ID;
		创建效果(所有玩家(所有队伍), 环, 蓝色, 全局.J, 1, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		等待(4, 无视条件);
		传送(全局.I, 全局.J);
		消除效果(事件玩家.XIAOG1);
		消除地图文本(事件玩家.WENBEN);
