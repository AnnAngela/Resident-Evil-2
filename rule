规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		黑影;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		全局.HEIY >= 事件玩家.GONGYONG;
	}

	动作
	{
		If(全局.HEIY == 0);
			事件玩家.GONGYONG = 4;
		Else If(全局.HEIY == 4);
			事件玩家.GONGYONG = 7;
			事件玩家.SH += 15;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			小字体信息(事件玩家, 自定义字符串("速度已增加15%"));
		Else If(全局.HEIY == 7);
			事件玩家.GONGYONG = 10;
			事件玩家.SM += 40;
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
			小字体信息(事件玩家, 自定义字符串("生命值增加40%"));
		Else If(全局.HEIY == 10);
			事件玩家.GONGYONG = 14;
			事件玩家.c = 12;
			小字体信息(事件玩家, 自定义字符串("隐身延长至12秒"));
		Else If(全局.HEIY == 14);
			事件玩家.GONGYONG = 20;
			小字体信息(事件玩家, 自定义字符串("逃生点位置已显示"));
			创建地图文本(事件玩家, 自定义字符串("{0}逃生点{1}", 技能图标字符串(英雄(黑影), 按钮(终极技能)), 技能图标字符串(英雄(黑影), 按钮(终极技能))), 全局.Y, 2, 不要截取, 可见，位置和字符串, 紫色, 默认可见度);
			事件玩家.WENBEN = 上一个文本ID;
		Else If(全局.HEIY == 20);
			小字体信息(事件玩家, 自定义字符串("已完成所有任务"));
		End;
	}
}

