规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		游戏正在进行中 == 真;
		比赛时间 < 375;
		全局.xiaomuti == 假;
		全局.K == 0;
	}

	动作
	{
		设置最大重生时间(全局.N, 6);
		消除地图文本(全局.xiaomuti3);
		设置主要攻击模式启用(全局.N, 假);
		If(全局.xiaomuti1 == 1);
			开始强制玩家选择英雄(全局.N, 英雄(奥丽莎));
		Else If(全局.xiaomuti1 == 2);
			开始强制玩家选择英雄(全局.N, 英雄(西格玛));
		End;
		小字体信息(全局.N, 自定义字符串("{已进化成为小母体}"));
		创建图标(所有玩家(所有队伍), 全局.N, 骷髅, 可见和位置, 橙色, 假);
		设置辅助攻击模式启用(全局.N, 真);
		消除地图文本(全局.N.TOUSHI);
		消除地图文本(全局.N.WENBEN);
		设置启用技能 1(全局.N, 真);
		等待(0.300, 无视条件);
		清除状态(全局.N, 冻结);
		设置造成伤害(全局.N, 100);
		设置移动速度(全局.N, 100);
		设置最大生命值(全局.N, 100);
		设置引力(全局.N, 100);
		设置不可见(全局.N, 无);
		设置受到伤害(全局.N, 100);
		全局.N.GONGJI = 0;
		可用按钮(全局.N, 终极技能);
		清除状态(全局.N, 无法杀死);
		设置弹道速度(全局.N, 100);
		消除效果(全局.N.XIAOG1);
		停止加速(全局.N);
		设置启用技能 2(全局.N, 真);
		解除绑定(全局.N);
		清除状态(全局.N, 消散);
		停止强制设置玩家位置(全局.N);
		可用按钮(全局.N, 跳跃);
		可用按钮(全局.N, 技能2);
	}
}

规则("")
{
	事件
	{
		玩家造成最后一击;
		双方;
		全部;
	}

	条件
	{
		数组包含(全局.a, 所用英雄(攻击方)) == 真;
		被攻击方 == 全局.N;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("{击杀高等感染体分数额外+2}"));
		修改玩家分数(事件玩家, 事件玩家 + 2);
		等待(2, 无视条件);
		If(所用英雄(被攻击方) == 英雄(西格玛));
			小字体信息(所有玩家(所有队伍), 自定义字符串("{0}被{1}击杀,获得大招", 英雄图标字符串(英雄(西格玛)), 所用英雄(攻击方)));
			等待(7, 无视条件);
			设置终极技能充能(被攻击方, 100);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		游戏正在进行中 == 真;
		全局.P.c == 10;
		(按钮被按下(全局.P, 主要攻击模式) == 真 || 按钮被按下(全局.P, 辅助攻击模式) == 真 || 按钮被按下(全局.P, 跳跃) == 真 || 按钮被按下(全局.P, 蹲下) == 真) == 真;
	}

	动作
	{
		消除地图文本(全局.X);
		全局.P.c = 1;
		If(按钮被按下(全局.P, 主要攻击模式) == 真);
			全局.P.b = 0;
			设置主要攻击模式启用(全局.P, 真);
			开始强制玩家选择英雄(全局.P, 英雄(莱因哈特));
			全局.Q = 2500;
		Else If(按钮被按下(全局.P, 辅助攻击模式) == 真);
			开始强制玩家选择英雄(全局.P, 英雄(温斯顿));
			设置状态(全局.P, 无, 消散, 1.500);
			全局.P.GONGJI = 0;
			等待(1.500, 无视条件);
			按下按键(全局.P, 终极技能);
			全局.Q = 2500;
		Else If(按钮被按下(全局.P, 跳跃) == 真);
			开始强制玩家选择英雄(全局.P, 英雄(D.Va));
			设置主要攻击模式启用(全局.P, 假);
			全局.Q = 3000;
			设置状态(全局.P, 无, 消散, 1.500);
			设置终极技能充能(全局.P, 100);
			等待(1, 无视条件);
			设置状态(全局.P, 无, 无法杀死, 600);
			按下按键(全局.P, 终极技能);
		Else If(按钮被按下(全局.P, 蹲下) == 真);
			开始强制玩家选择英雄(全局.P, 英雄(破坏球));
			设置主要攻击模式启用(全局.P, 假);
			全局.Q = 3000;
		End;
	}
}

