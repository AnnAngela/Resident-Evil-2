	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		半藏;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
		终极技能充能百分比(事件玩家) >= 15;
	}

	动作
	{
		施加推力(事件玩家, 地图矢量(矢量(0, 1, 1), 事件玩家, 旋转), 24, 至地图, 取消相反运动);
		设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) - 15);
		小字体信息(事件玩家, 自定义字符串("已消耗15%能量,释放龙腾"));
		播放效果(所有玩家(所有队伍), 环状爆炸, 黄色, 事件玩家, 2);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 55);
		等待(3, 无视条件);
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		游戏正在进行中 == 真;
		玩家数量(所有队伍) > 8;
		实体存在(全局.N) == 假;
		比赛时间 < 340;
		数量(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.S, 所用英雄(当前数组元素)))) > 0;
	}

	动作
	{
		大字体信息(所有玩家(所有队伍), 自定义字符串("小母体离开游戏,小母体身份被随机继承"));
		全局.N = 数组随机取值(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.S, 所用英雄(当前数组元素))));
		等待(1.500, 无视条件);
		开始强制玩家选择英雄(全局.N, 英雄(西格玛));
		设置辅助攻击模式启用(全局.N, 真);
		等待(1.500, 无视条件);
		设置造成伤害(全局.N, 100);
		设置辅助攻击模式启用(全局.N, 真);
		设置主要攻击模式启用(全局.N, 假);
		设置弹道速度(全局.N, 100);
		设置引力(全局.N, 100);
		设置不可见(全局.N, 无);
		设置引力(全局.N, 100);
	}
}

