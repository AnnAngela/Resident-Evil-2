		事件玩家.SH += 30;
		设置移动速度(事件玩家, 事件玩家.SH + 100);
		开始持续治疗(事件玩家, 事件玩家, 3, 20);
		等待(3, 无视条件);
		事件玩家.SH -= 30;
		设置移动速度(事件玩家, 事件玩家.SH + 100);
	}
