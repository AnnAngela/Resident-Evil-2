规则("")
{
	事件
	{
		玩家受到治疗;
		双方;
		全部;
	}

	条件
	{
		事件为急救包 == 真;
		数组包含(全局.a, 所用英雄(事件玩家)) == 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("{吃掉血包，你的速度短暂增加了}"));
		事件玩家.SH += 45;
		设置移动速度(事件玩家, 事件玩家.SH + 100);
		事件玩家.SUIJI = 随机整数(0, 9);
		等待(0.300, 无视条件);
		If(所用英雄(事件玩家) == 英雄(士兵：76) && 事件玩家.XUANZHE == 1);
			事件玩家.SUIJI = 1;
		End;
		If(事件玩家.SUIJI < 5);
			小字体信息(事件玩家, 自定义字符串("{该血包质量为“优”，生命值上限永久增加了}"));
			事件玩家.SM += 5;
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
			等待(0.200, 无视条件);
			治疗(事件玩家, 无, 2000);
		End;
		等待(2, 无视条件);
		事件玩家.SH -= 45;
		设置移动速度(事件玩家, 事件玩家.SH + 100);
	}
}

