规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		奥丽莎;
	}

	条件
	{
		正在使用终极技能(事件玩家) == 真;
	}

	动作
	{
		大字体信息(范围内玩家(事件玩家, 25, 所有队伍, 关闭), 自定义字符串("奥丽莎正在使用震地,持续伤害周围人"));
		全局.AOLI = 所选位置(事件玩家);
		创建效果(所有玩家(所有队伍), 球体, 红色, 全局.AOLI, 13, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		While(正在使用终极技能(事件玩家) == 真);
			等待(0.800, 无视条件);
			伤害(范围内玩家(全局.AOLI, 13, 所有队伍, 关闭), 事件玩家, 25);
			播放效果(所有玩家(所有队伍), 有害爆炸, 红色, 全局.AOLI, 13);
			播放效果(所有玩家(所有队伍), 环状爆炸, 红色, 全局.AOLI, 13);
			播放效果(所有玩家(所有队伍), 爆炸声音, 红色, 全局.AOLI, 120);
			等待(1.200, 无视条件);
		End;
		消除效果(事件玩家.XIAOG1);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		艾什;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
		存活(事件玩家) == 真;
	}

	动作
	{
		事件玩家.c = 射线命中位置(眼睛位置(事件玩家), 眼睛位置(事件玩家) + 面朝方向(事件玩家) * 25, 所有玩家(所有队伍), 事件玩家, 真);
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 60);
		小字体信息(事件玩家, 自定义字符串("已释放火球术,技能进入冷却20秒"));
		创建效果(所有玩家(所有队伍), 球, 红色, 事件玩家.a, 2, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		事件玩家.a = 眼睛位置(事件玩家);
		持续追踪玩家变量(事件玩家, a, 事件玩家.c, 1, 无);
		等待(1, 无视条件);
		消除效果(事件玩家.XIAOG1);
		播放效果(所有玩家(所有队伍), 有害爆炸, 红色, 事件玩家.a, 6);
		播放效果(所有玩家(所有队伍), 环状爆炸, 红色, 事件玩家.a, 7);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 红色, 事件玩家.a, 6);
		伤害(范围内玩家(事件玩家.a, 6, 所有队伍, 关闭), 事件玩家, 600);
		设置状态(范围内玩家(事件玩家.a, 6, 所有队伍, 关闭), 无, 点燃, 2);
		停止追踪玩家变量(事件玩家, a);
		等待(20, 无视条件);
		小字体信息(事件玩家, 自定义字符串("火球术已冷却完毕"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 60);
	}
}

