		创建地图文本(全局.N, 自定义字符串("        选择变异方向\r\n鼠标左键       鼠标右键\r\n(奥丽莎)         (西格玛)"), 眼睛位置(全局.N) + 面朝方向(全局.N) * 1, 2.500, 不要截取,
			可见，位置和字符串, 橙色, 默认可见度);
		全局.xiaomuti3 = 上一个文本ID;
		设置目标点描述(所有玩家(所有队伍), 自定义字符串("母体已经降临,人类的命运该会如何呢？\r\n            剩余人类数量 {0} / {1} ", 数量(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.a, 所用英雄(当前数组元素)))),
			玩家数量(所有队伍)), 可见性，排序规则，以及字符串);
		创建地图文本(全局.P, 自定义字符串("             按住按钮选择变异方向\r\n鼠标左键      跳跃       蹲下     鼠标右键   \r\n(莱因哈特)  (D.VA)  (破环球)   (温斯顿)"), 眼睛位置(全局.P)
			+ 面朝方向(全局.P) * 1, 2.500, 不要截取, 可见，位置和字符串, 橙色, 默认可见度);
		全局.X = 上一个文本ID;
		等待(2, 无视条件);
		全局.P.c = 10;
		全局.P.W = 0;
		等待(10, 无视条件);
		按下按键(全局.N, 主要攻击模式);
		设置受到治疗(全局.P, 0);
		If(全局.P.c == 10);
			开始强制玩家选择英雄(全局.P, 英雄(莱因哈特));
			消除地图文本(全局.X);
			全局.P.c = 1;
			全局.Q = 2500;
		End;
	}
}

