规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		安娜;
	}

	条件
	{
		事件玩家.XUANZHE == 3;
		按钮被按下(事件玩家, 互动) == 真;
		具有状态(事件玩家, 消散) != 真;
		数组包含(从数组中移除(从数组中移除(全局.a, 所用英雄(全局.P)), 所用英雄(全局.N)), 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) == 真;
		在视线内(事件玩家, 距离准星最近的玩家(事件玩家, 所有队伍), 屏障不会阻挡视线) == 真;
	}

	动作
	{
		全局.U = 距离准星最近的玩家(事件玩家, 所有队伍);
		等待(0.100, 无视条件);
		传送(事件玩家, 全局.U);
		设置状态(事件玩家, 无, 消散, 600);
		等待(0.100, 无视条件);
		绑定玩家(事件玩家, 全局.U, 矢量(0, 1.200, 0));
		全局.U.SM += 30;
		小字体信息(全局.U, 自定义字符串("被认主,获得30%生命值加成"));
		设置最大生命值(全局.U, 全局.U.SM + 100);
		小字体信息(事件玩家, 自定义字符串("已选择{0}作为主人", 所用英雄(全局.U)));
		消除地图文本(事件玩家.TOUSHI);
		设置不可见(事件玩家, 全部);
		创建效果(所有玩家(所有队伍), 火花, 绿色, 所选位置(事件玩家), 1, 可见，位置和半径);
		事件玩家.XIAOG1 = 最后创建的实体;
		修改全局变量(RL, 添加至数组, 英雄(安娜));
		禁用按钮(事件玩家, 技能2);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		游戏正在进行中 == 真;
		数组包含(全局.disan, 所用英雄(事件玩家)) == 真;
		事件玩家.XUANZHE == 0;
		正在蹲下(事件玩家) == 真;
	}

	动作
	{
		If(分数(事件玩家) >= 10);
			小字体信息(事件玩家, 自定义字符串("正在选择职业中···③"));
			等待(1, 当为“假”时中止);
			设置主要攻击模式启用(事件玩家, 真);
			设置辅助攻击模式启用(事件玩家, 真);
			If(所用英雄(事件玩家) == 英雄(安娜));
				事件玩家.WENzi = 自定义字符串(
					"认主(主动):选择(鼠标最近)人类当自己的主人,主人会\r\n获得生命加成.主人死亡你也会死亡(仅可使用一次)\r\n{0}冷却缩短至5秒\r\n{1}为主人回满血并令主人无敌8秒\r\n{2}为主人恢复70生命并使其无敌1秒", 技能图标字符串(
					英雄(安娜), 按钮(技能1)), 技能图标字符串(英雄(安娜), 按钮(终极技能)), 技能图标字符串(英雄(安娜), 按钮(技能2)));
				设置最大生命值(事件玩家, 60);
			Else If(所用英雄(事件玩家) == 英雄(黑百合));
				事件玩家.WENzi = 自定义字符串("{0}使用后增加50%速度持续3秒\r\n失智(被动):火力全开状态可狙击\r\n并且伤害增加50%\r\n狂暴(被动):生命值低于50%获得\r\n80%伤害加成", 技能图标字符串(英雄(黑百合), 按钮(
					技能1)), 技能图标字符串(英雄(黑百合), 按钮(技能2)), 技能图标字符串(英雄(黑影), 按钮(近身攻击)));
				事件玩家.GONGJI += 100;
				设置终极技能充能(事件玩家, 100);
				设置辅助攻击模式启用(事件玩家, 假);
				设置弹道速度(事件玩家, 500);
				设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
			Else If(所用英雄(事件玩家) == 英雄(回声));
				事件玩家.WENzi = 自定义字符串("附着(主动):无视距离,指定一个人类,附着在其背后\r\n蹲下解除.冷却解除附着后6秒\r\n{0}可复制感染体(不可母体),每次复制后都会提升属性", 技能图标字符串(英雄(回声), 按钮(终极技能)));
			Else If(所用英雄(事件玩家) == 英雄(源氏));
				事件玩家.WENzi = 自定义字符串("{0}期间被击退会反击攻击者,对其造成伤害并晕眩\r\n{1}每次造成伤害会恢复10%已损失生命值\r\n并附带范围伤害\r\n击杀敌人后,敌人尸体会爆炸,对周围敌人造成伤害", 技能图标字符串(英雄(源氏), 按钮(
					技能2)), 技能图标字符串(英雄(源氏), 按钮(终极技能)));
				设置终极技能充能(事件玩家, 100);
				按下按键(事件玩家, 终极技能);
			End;
			小字体信息(事件玩家, 自定义字符串("已选择完毕,请善用你的技能"));
			播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 75);
			事件玩家.XUANZHE = 3;
			修改玩家分数(事件玩家, 事件玩家 - 10);
		Else;
			小字体信息(事件玩家, 自定义字符串("分数不足···"));
			等待(4, 无视条件);
		End;
	}
}

