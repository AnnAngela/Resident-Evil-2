规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		比赛时间 < 370;
		游戏正在进行中 == 真;
		数量(已过滤的数组(所有玩家(所有队伍), 数组包含(全局.b, 所用英雄(当前数组元素)))) == 玩家数量(所有队伍);
		全局.HUIHEJIESHU1 != 真;
		全局.TAOWANG > 0;
	}

	动作
	{
		大字体信息(所有玩家(所有队伍), 自定义字符串("场上存活的人类已全部逃离"));
		等待(1.800, 无视条件);
		全局.HUIHEJIESHU = 1;
		If(全局.TAOWANG == 1);
			设置目标点描述(所有玩家(所有队伍), 自定义字符串("(结局3)在感染体猛攻下,仅有一名人类逃了\r\n出来,感染体基本掌握了这颗星球(感染大胜)\r\n母体+2分    小母体+4分   逃离人类+10分"), 字符串);
			修改玩家分数(全局.P, 全局.P + 2);
			修改玩家分数(全局.N, 全局.N + 4);
		Else If(全局.TAOWANG == 2);
			设置目标点描述(所有玩家(所有队伍), 自定义字符串("(结局4)感染体的疯狂杀戮着人类,二名人类趁乱\r\n逃了出来,在偏僻地方苟延残喘(人类惨败)\r\n母体+1分    小母体+2分   逃离人类+10分"), 字符串);
			修改玩家分数(全局.P, 全局.P + 1);
			修改玩家分数(全局.N, 全局.N + 2);
		Else If(全局.TAOWANG <= 4);
			设置目标点描述(所有玩家(所有队伍), 自定义字符串("(结局5)人类在对抗感染体中伤亡惨重,小部分人\r\n逃离了这座城市,在某个地方积蓄力量(人类惜败)\r\n母体+0分    小母体+0分   逃离人数+10分"), 字符串);
		Else;
			设置目标点描述(所有玩家(所有队伍), 自定义字符串("(结局6)人类团结地对抗着感染体,大部分人类\r\n活着逃离出来,感染没有散播出去(感染惨败)\r\n母体-4分    小母体-2分   逃离人数+10分"), 字符串);
			修改玩家分数(全局.P, 全局.P - 4);
			修改玩家分数(全局.N, 全局.N - 2);
		End;
		等待(1, 无视条件);
		设置慢动作(100);
		全局.HUIHEJIESHU1 = 真;
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
