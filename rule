	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		禅雅塔;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		游戏正在进行中 == 真;
		全局.TSD == 真;
		比赛时间 < 370;
		正在蹲下(事件玩家) == 真;
		具有状态(事件玩家, 被入侵) != 真;
	}

	动作
	{
		小字体信息(所有玩家(所有队伍), 自定义字符串("{0}正在转移{1}", 英雄图标字符串(英雄(禅雅塔)), 技能图标字符串(英雄(秩序之光), 按钮(技能2))));
		播放效果(所有玩家(所有队伍), 状态爆炸声音, 白色, 事件玩家, 200);
		等待(4, 当为“假”时中止);
		小字体信息(事件玩家, 自定义字符串("逃生点转移剩余2秒"));
		等待(1, 当为“假”时中止);
		小字体信息(事件玩家, 自定义字符串("逃生点转移剩余1秒"));
		等待(1, 当为“假”时中止);
		全局.Y = 所选位置(事件玩家);
		播放效果(所有玩家(所有队伍), 状态爆炸声音, 白色, 事件玩家, 200);
		大字体信息(所有玩家(所有队伍), 自定义字符串("{0}逃生点已转移", 英雄图标字符串(英雄(禅雅塔))));
		小字体信息(事件玩家, 自定义字符串("转移逃生点完毕,分数+4"));
		修改玩家分数(事件玩家, 事件玩家 + 5);
	}
}

规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		禅雅塔;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		数组包含(全局.b, 所用英雄(被攻击方)) == 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("已使用醒脑,技能进入冷却2秒"));
		施加推力(被攻击方, 地图矢量(矢量(0, 1, 0), 被攻击方, 旋转), 40, 至地图, 取消相反运动);
		等待(0.200, 无视条件);
		开始强制设置玩家位置(被攻击方, 被攻击方, 真);
		等待(1.500, 无视条件);
		停止强制设置玩家位置(被攻击方);
		等待(2, 无视条件);
		小字体信息(事件玩家, 自定义字符串("醒脑冷却完毕,可再次使用"));
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		狂鼠;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		正在使用终极技能(事件玩家) == 真;
	}

	动作
	{
		设置状态(事件玩家, 无, 消散, 12);
		创建地图文本(所有玩家(所有队伍), 自定义字符串("无法被伤害,正在换位中"), 事件玩家, 1.400, 根据表面截取, 可见，位置和字符串, 橙色, 默认可见度);
		事件玩家.WENBEN = 上一个文本ID;
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		狂鼠;
	}

	条件
	{
		事件技能 != 按钮(主要攻击模式);
		事件技能 != 按钮(近身攻击);
	}

	动作
	{
		If(事件玩家.XUANZHE == 2 && 事件技能 == 按钮(终极技能));
			事件玩家.GONGYONG = 所选位置(事件玩家);
			传送(事件玩家, 被攻击方);
			等待(0.100, 无视条件);
			传送(被攻击方, 事件玩家.GONGYONG);
			小字体信息(事件玩家, 自定义字符串("已换位"));
			小字体信息(被攻击方, 自定义字符串("被狂鼠大招击中,已跟狂鼠换位"));
			清除状态(事件玩家, 消散);
			消除地图文本(事件玩家.WENBEN);
		Else If(事件玩家.XUANZHE == 1 && 事件技能 == 按钮(技能1));
			If(被攻击方 != 全局.P);
				事件玩家.GONGYONG = 所选位置(事件玩家);
				传送(事件玩家, 被攻击方);
				等待(0.030, 无视条件);
				传送(被攻击方, 事件玩家.GONGYONG);
				小字体信息(事件玩家, 自定义字符串("成功命中敌人,已换位"));
				等待(0.010, 无视条件);
			End;
			设置状态(被攻击方, 事件玩家, 昏迷, 1.500);
		Else If(事件玩家.XUANZHE == 1 && 所用英雄(被攻击方) != 英雄(堡垒) && 事件技能 == 按钮(技能2));
			小字体信息(事件玩家, 技能图标字符串(英雄(狂鼠), 按钮(技能2)));
			伤害(被攻击方, 事件玩家, 500);
		End;
	}
}

