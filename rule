规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		卢西奥;
	}

	条件
	{
		生命值(事件玩家) >= 800;
	}

	动作
	{
		If(事件玩家.XUANZHE == 1);
			设置终极技能充能(事件玩家, 10);
			施加推力(事件玩家, 矢量(0, 500, 0), 25, 至地图, 取消相反运动);
			小字体信息(事件玩家, 自定义字符串("已对自己施加风翔术,速度大幅提高"));
			事件玩家.SH += 80;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			等待(4, 无视条件);
			事件玩家.SH -= 80;
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			等待(3, 无视条件);
		Else;
			小字体信息(所有玩家(所有队伍), 自定义字符串("{0}所有人类化雾16秒", 英雄图标字符串(英雄(卢西奥))));
			修改玩家分数(事件玩家, 事件玩家 + 5);
		End;
	}
}

