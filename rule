规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		温斯顿;
	}

	条件
	{
		(具有状态(事件玩家, 击倒) || 具有状态(事件玩家, 昏迷) || 具有状态(事件玩家, 冻结) || 具有状态(事件玩家, 沉睡)) == 真;
	}

	动作
	{
		等待(0.800, 当为“假”时中止);
		清除状态(事件玩家, 昏迷);
		清除状态(事件玩家, 冻结);
		清除状态(事件玩家, 沉睡);
		清除状态(事件玩家, 击倒);
		设置技能冷却(事件玩家, 按钮(技能1), 技能冷却时间(事件玩家, 按钮(技能1)) * 0.300);
		事件玩家.GONGJI += 2;
		设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
	}
}

规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		西格玛;
	}

	条件
	{
		数组包含(全局.a, 所用英雄(被攻击方)) == 真;
	}

	动作
	{
		If(事件技能 == 按钮(技能2));
			设置状态(被攻击方, 事件玩家, 昏迷, 1.250);
			等待(0.600, 无视条件);
			设置技能冷却(事件玩家, 按钮(技能2), 技能冷却时间(事件玩家, 按钮(技能2)) - 2);
		Else If(事件技能 == 按钮(近身攻击));
			伤害(被攻击方, 事件玩家, 30);
		End;
	}
}

规则("")
{
	事件
	{
		玩家造成最后一击;
		双方;
		全部;
	}

	条件
	{
		(事件玩家 == 全局.BD1 || 事件玩家 == 全局.U) == 真;
	}

	动作
	{
		If(事件玩家 == 全局.BD1);
			修改玩家分数(选择英雄的玩家(英雄(巴蒂斯特), 所有队伍), 选择英雄的玩家(英雄(巴蒂斯特), 所有队伍) + 2);
			小字体信息(选择英雄的玩家(英雄(巴蒂斯特), 所有队伍), 自定义字符串("你的对象击杀敌人,你的分数+2"));
		End;
		If(事件玩家 == 全局.U);
			修改玩家分数(选择英雄的玩家(英雄(安娜), 所有队伍), 选择英雄的玩家(英雄(安娜), 所有队伍) + 2);
			小字体信息(选择英雄的玩家(英雄(安娜), 所有队伍), 自定义字符串("你的主人击杀敌人,你的分数+2"));
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
