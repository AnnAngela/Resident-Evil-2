	{
		设置技能冷却(事件玩家, 按钮(技能2), 技能冷却时间(事件玩家, 按钮(技能2)) / 2);
		小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(黑百合), 按钮(技能2)), 自定义字符串("已返50%冷却")));
		设置状态(被攻击方, 事件玩家, 昏迷, 2);
		等待(5, 无视条件);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		黑百合;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
		事件玩家.W == 假;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("已为下一发子弹附魔"));
		事件玩家.a = 2;
		事件玩家.W = 真;
		播放效果(事件玩家, 爆炸声音, 白色, 事件玩家, 100);
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		黑百合;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		事件玩家.a == 2;
		事件技能 == 按钮(主要攻击模式);
		正在使用辅助武器(事件玩家) == 真;
	}

	动作
	{
		If(事件暴击 == 真);
			伤害(被攻击方, 事件玩家, 1000);
			小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(黑百合), 按钮(终极技能)), 自定义字符串("(爆头)致命一击！")));
		Else;
			伤害(被攻击方, 事件玩家, 250);
		End;
		播放效果(所有玩家(所有队伍), 有害爆炸, 红色, 眼睛位置(被攻击方), 1);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 红色, 眼睛位置(被攻击方), 100);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		黑百合;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		正在使用主要武器(事件玩家) == 真;
		事件玩家.a == 2;
	}

	动作
	{
		等待(0.100, 无视条件);
		事件玩家.a = 0;
		小字体信息(事件玩家, 自定义字符串("强化子弹已射出,技能进入冷却8秒"));
		等待(8, 无视条件);
		小字体信息(事件玩家, 自定义字符串("{专注技能冷却完毕}"));
		事件玩家.W = 假;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		黑百合;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(猎空), 按钮(技能1)), 自定义字符串("已使用逃亡技能,逃亡进入冷却5秒")));
		设置技能冷却(事件玩家, 按钮(技能1), 0);
		播放效果(所有玩家(所有队伍), 环状爆炸, 紫色, 事件玩家, 5);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 75);
		施加推力(事件玩家, 地图矢量(矢量(0, 1, -1), 事件玩家, 旋转), 22, 至地图, 取消相反运动);
		等待(0.100, 无视条件);
		设置移动速度(事件玩家, 1500);
		开始定向阈值(事件玩家, 地图矢量(矢量(0, 1, -1), 事件玩家, 旋转), 1, 至地图, 替换现有阈值, 无);
		等待(0.050, 无视条件);
		停止定向阈值(事件玩家);
		设置移动速度(事件玩家, 100);
		等待(5, 无视条件);
		小字体信息(事件玩家, 字符串("{0} {1}", 技能图标字符串(英雄(猎空), 按钮(技能1)), 自定义字符串("逃亡技能冷却就绪,可再次释放")));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 60);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		士兵：76;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
		终极技能充能百分比(事件玩家) >= 15;
		具有状态(事件玩家, 被入侵) != 真;
	}

	动作
	{
		事件玩家.b = 自定义字符串("");
		事件玩家.GONGJI = 事件玩家.GONGJI - 全局.SUIJI1;
		事件玩家.SM = 事件玩家.SM - 全局.SUIJI1;
		事件玩家.SH = 事件玩家.SH - 全局.SUIJI1 / 4;
		播放效果(事件玩家, 爆炸声音, 白色, 事件玩家, 75);
		小字体信息(事件玩家, 自定义字符串("{正在投骰子~~}"));
		设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) - 15);
		等待(0.500, 无视条件);
		全局.SUIJI1 = 随机整数(1, 120);
		等待(0.500, 无视条件);
		小字体信息(事件玩家, 自定义字符串("你所投出的骰子点数为{0}", 全局.SUIJI1));
		If(全局.SUIJI1 <= 20);
			事件玩家.c += 1;
			事件玩家.b = 自定义字符串("点数<21,霉运能量+1,目前霉运能量:{0}\r\n(当霉运能量到达3时,你等着瞧)", 事件玩家.c);
			小字体信息(事件玩家, 自定义字符串("点数太小，属性无任何提升\r\n返还15%能量(恶魔的**)"));
			设置终极技能充能(事件玩家, 终极技能充能百分比(事件玩家) + 15);
		Else;
			播放效果(事件玩家, 状态爆炸声音, 白色, 事件玩家, 100);
			事件玩家.SH = 事件玩家.SH + 全局.SUIJI1 / 4;
			事件玩家.SM = 事件玩家.SM + 全局.SUIJI1;
			事件玩家.GONGJI = 事件玩家.GONGJI + 全局.SUIJI1;
			小字体信息(事件玩家, 自定义字符串("已根据你的点数为你更改属性"));
			设置移动速度(事件玩家, 事件玩家.SH + 100);
			设置最大生命值(事件玩家, 事件玩家.SM + 100);
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
			小字体信息(事件玩家, 自定义字符串("你的生命与攻击提高了{0}%", 全局.SUIJI1));
			小字体信息(事件玩家, 自定义字符串("你的速度提高了{0}%", 事件玩家.SH));
		End;
		If(全局.SUIJI1 > 99);
			事件玩家.a += 1;
			事件玩家.b = 自定义字符串("点数＞99,幸运能量+1,目前幸运能量:{0}\r\n(幸运能量到达3会有意想不到事情发生噢)", 事件玩家.a);
		End;
		If(事件玩家.a == 3);
			大字体信息(所有玩家(所有队伍), 自定义字符串("洪福齐天,未知的时空裂痕将{0}成功转移至逃生点", 英雄图标字符串(英雄(士兵：76))));
			修改玩家分数(事件玩家, 事件玩家 + 16);
			调用子程序(Sub0);
		Else If(事件玩家.c == 3);
			大字体信息(所有玩家(所有队伍), 自定义字符串("未知的时空裂痕将{0}转移至地球外，士兵.76 死", 英雄图标字符串(英雄(士兵：76))));
			等待(1, 无视条件);
			传送(事件玩家, 矢量(0, -350, 0));
		End;
		等待(2, 无视条件);
	}
}

规则("")
{
	事件
	{
		玩家造成击退;
		双方;
		士兵：76;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		数组包含(从数组中移除(全局.a, 英雄(士兵：76)), 所用英雄(被攻击方)) != 真;
		事件技能 != 按钮(近身攻击);
	}

	动作
	{
		If(数组包含(全局.b, 所用英雄(被攻击方)) == 真);
			设置状态(被攻击方, 事件玩家, 昏迷, 2);
		End;
		If(被攻击方 == 事件玩家);
			设置移动速度(事件玩家, 事件玩家.SH + 200);
			等待(1.500, 无视条件);
			设置移动速度(事件玩家, 事件玩家.SH + 100);
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		堡垒;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
		事件玩家.W == 0;
	}

	动作
	{
		If(数量(全局.c) != 0);
			事件玩家.W = 1;
			等待(50, 无视条件);
			小字体信息(事件玩家, 自定义字符串("可以再次强化自身"));
		Else;
			小字体信息(事件玩家, 自定义字符串("零件已全部强化,不可继续强化"));
			等待(5, 无视条件);
		End;
	}
}

