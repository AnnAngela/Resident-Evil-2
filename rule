规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		秩序之光;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件玩家.c == 1;
		按钮被按下(事件玩家, 主要攻击模式) == 真;
	}

	动作
	{
		If(事件玩家.a == 全局.F);
			事件玩家.c = 2;
			修改玩家分数(事件玩家, 事件玩家 + 20);
			大字体信息(所有玩家(所有队伍), 自定义字符串("{0}秩序破解了密 码，逃离了此处", 英雄图标字符串(英雄(秩序之光))));
			调用子程序(Sub0);
		Else;
			大字体信息(事件玩家, 自定义字符串("密 码错误"));
			事件玩家.c = 0;
			设置启用技能 2(事件玩家, 真);
		End;
	}
}

规则("")
{
	事件
	{
		子程序;
		Sub0;
	}

	动作
	{
		If(事件玩家 == 全局.BD1);
			修改玩家分数(选择英雄的玩家(英雄(巴蒂斯特), 所有队伍), 选择英雄的玩家(英雄(巴蒂斯特), 所有队伍) + 5);
			小字体信息(选择英雄的玩家(英雄(巴蒂斯特), 所有队伍), 自定义字符串("你的对象成功逃离,你的分数+5"));
		End;
		If(事件玩家 == 全局.O);
			修改玩家分数(选择英雄的玩家(英雄(回声), 所有队伍), 选择英雄的玩家(英雄(回声), 所有队伍) + 15);
			修改全局变量(b, 添加至数组, 英雄(回声));
			修改全局变量(a, 根据值从数组中移除, 英雄(回声));
		End;
		If(事件玩家 == 全局.U);
			修改玩家分数(选择英雄的玩家(英雄(安娜), 所有队伍), 选择英雄的玩家(英雄(安娜), 所有队伍) + 25);
			修改全局变量(b, 添加至数组, 英雄(安娜));
			修改全局变量(a, 根据值从数组中移除, 英雄(安娜));
		End;
		修改全局变量(a, 根据值从数组中移除, 所用英雄(事件玩家));
		设置受到伤害(事件玩家, 0);
		设置造成伤害(事件玩家, 0);
		设置状态(事件玩家, 无, 定身, 0.500);
		传送(事件玩家, 全局.TL);
		小字体信息(事件玩家, 自定义字符串("成功逃离，分数+10"));
		全局.TAOWANG += 1;
		小字体信息(所有玩家(所有队伍), 字符串("{0} {1}", 所用英雄(事件玩家), 自定义字符串("已逃离")));
		修改全局变量(RL, 添加至数组, 所用英雄(事件玩家));
		设置状态(事件玩家, 无, 被入侵, 9999);
		消除地图文本(事件玩家.TOUSHI);
		事件玩家.XUANZHE = -1;
		等待(0.250, 无视条件);
		修改全局变量(b, 添加至数组, 所用英雄(事件玩家));
	}
}

