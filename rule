规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		半藏;
	}

	条件
	{
		游戏正在进行中 == 真;
		事件玩家.XUANZHE == 0;
	}

	动作
	{
		事件玩家.WENzi = 自定义字符串("请选择您的职业\r\n隐士[按住鼠标左键2秒]\r\n刺客[按住鼠标右键2秒]");
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		正在交流(事件玩家, 终极技能状态) == 真;
	}

	动作
	{
		If(事件玩家.TISHI == 0);
			修改全局变量(wenzi5, 根据值从数组中移除, 事件玩家);
			事件玩家.TISHI = 1;
		Else;
			修改全局变量(wenzi5, 添加至数组, 事件玩家);
			事件玩家.TISHI = 0;
		End;
		等待(3, 无视条件);
	}
}

规则("")
{
	事件
	{
		玩家造成伤害;
		双方;
		秩序之光;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		事件技能 == 按钮(辅助攻击模式);
	}

	动作
	{
		设置状态(被攻击方, 事件玩家, 昏迷, 0.350);
		播放效果(所有玩家(所有队伍), 环状爆炸, 蓝色, 被攻击方, 3);
		等待(0.016, 无视条件);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		天使;
	}

