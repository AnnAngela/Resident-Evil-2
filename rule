规则("DJ2")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		卢西奥;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("使用雾行中···"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		设置引力(事件玩家, 0);
		开始加速(事件玩家, 地图矢量(矢量(0, 0, 1), 事件玩家, 旋转), 10, 6, 至地图, 方向，速率，及最大速度);
		等待(6, 无视条件);
		停止加速(事件玩家);
		设置引力(事件玩家, 100);
		小字体信息(事件玩家, 自定义字符串("雾行效果结束,技能进入冷却22秒"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
		等待(22, 无视条件);
		小字体信息(事件玩家, 自定义字符串("雾行冷却完毕，可再次释放"));
	}
}

