规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		全局.HUIHEJIESHU >= 1;
	}

	动作
	{
		全局.FS1 = 空数组;
		可用按钮(所有玩家(所有队伍), 技能2);
		全局.busi = 空数组;
		全局.FAJI2 = 假;
		设置弹道速度(所有玩家(所有队伍), 100);
		清除状态(所有玩家(所有队伍), 无法杀死);
		停止定向阈值(所有玩家(所有队伍));
		可用按钮(所有玩家(所有队伍), 终极技能);
		设置启用技能 2(所有玩家(所有队伍), 真);
		全局.HUIHEJIESHU = 0;
		全局.Z += 1;
		等待(0.100, 无视条件);
		全局.BD1 = 无;
		等待(0.100, 无视条件);
		If(全局.Z <= 4);
			全局.FAJI = 0;
			大字体信息(所有玩家(所有队伍), 自定义字符串("{进入下一回合}"));
			等待(1.500, 无视条件);
			全局.U = 空数组;
			前往集结英雄;
			所有玩家(所有队伍).WENzi = 自定义字符串("");
			等待(2.500, 无视条件);
			所有玩家(所有队伍).GONGJI = 0;
			停止强制玩家选择英雄(所有玩家(所有队伍));
			比赛时间继续;
			所有玩家(所有队伍).a = 0;
			设置弹道速度(所有玩家(所有队伍), 100);
			设置受到伤害(所有玩家(所有队伍), 100);
			设置造成伤害(所有玩家(所有队伍), 100);
			设置最大生命值(所有玩家(所有队伍), 100);
			设置启用技能 1(所有玩家(所有队伍), 真);
			设置不可见(所有玩家(所有队伍), 无);
			等待(4.500, 无视条件);
			消除所有地图文本;
			消除所有效果;
			等待(1, 无视条件);
			设置受到治疗(所有玩家(所有队伍), 100);
			全局.RS = 0;
			所有玩家(所有队伍).SM = 0;
			设置主要攻击模式启用(所有玩家(所有队伍), 真);
			设置辅助攻击模式启用(所有玩家(所有队伍), 真);
			所有玩家(所有队伍).b = 0;
			所有玩家(所有队伍).SH = 0;
			设置移动速度(所有玩家(所有队伍), 100);
			所有玩家(所有队伍).GONGYONG = 0;
			全局.FS = 首个(已排序的数组(所有玩家(所有队伍), 分数(当前数组元素) * -1));
			全局.BZ = 矢量(0, -500, 0);
			消除所有图标;
			所有玩家(所有队伍).MAOMEI = 0;
			所有玩家(所有队伍).c = 0;
			调用子程序(Su);
			所有玩家(所有队伍).W = 假;
			清除状态(所有玩家(所有队伍), 被入侵);
			设置引力(所有玩家(所有队伍), 100);
			全局.QR = 假;
			全局.HUIHEJIESHU = 0;
			全局.TAOWANG = 0;
			全局.SUIJI1 = 0;
			全局.JYZ = 0;
			等待(0.500, 无视条件);
			设置最大重生时间(所有玩家(所有队伍), 6);
			全局.TSD = 假;
			全局.SHIJIAN = 假;
			全局.JS = 0;
			设置造成治疗(所有玩家(所有队伍), 100);
			停止加速(所有玩家(所有队伍));
			全局.G = 0;
			设置启用技能 2(所有玩家(所有队伍), 真);
			解除绑定(所有玩家(所有队伍));
			可用按钮(所有玩家(所有队伍), 跳跃);
			停止强制设置玩家位置(所有玩家(所有队伍));
			全局.HEIY = 0;
			修改玩家分数(所有玩家(所有队伍), 所有玩家(所有队伍) + 5);
			停止追踪玩家变量(所有玩家(所有队伍), a);
			全局.D = 空数组;
			清除状态(所有玩家(所有队伍), 消散);
			所有玩家(所有队伍).E = 0;
			全局.O = 无;
			所有玩家(所有队伍).ddongjie = 假;
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		全局.Z >= 5;
	}

	动作
	{
		停止镜头(所有玩家(所有队伍));
		等待(1, 无视条件);
		创建效果(所有玩家(所有队伍), 有益光环, 黄色, 全局.SL, 1, 可见，位置和半径);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 全局.JT, 200);
		设置状态(所有玩家(所有队伍), 无, 定身, 50);
		全局.SL = 首个(已排序的数组(所有玩家(所有队伍), 分数(当前数组元素) * -1));
		等待(0.100, 无视条件);
		全局.JT1 = 射线命中位置(眼睛位置(全局.SL), 眼睛位置(全局.SL) + 面朝方向(全局.SL) * 1.200, 所有玩家(所有队伍), 全局.SL, 真);
		全局.JT = 矢量(0, 100, 0);
		大字体信息(所有玩家(所有队伍), 字符串("{0} {1} {2}", 自定义字符串("恭喜"), 全局.SL, 自定义字符串("这名玩家取得胜利")));
		开始镜头(所有玩家(所有队伍), 全局.JT, 眼睛位置(全局.SL), 25);
		持续追踪全局变量(JT, 全局.JT1, 5, 无);
		等待(7, 无视条件);
		宣告玩家胜利(首个(已排序的数组(所有玩家(所有队伍), 分数(当前数组元素) * -1)));
	}
}

