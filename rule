规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		莫伊拉;
	}

	条件
	{
		事件玩家.XUANZHE == 1;
		按钮被按下(事件玩家, 互动) == 真;
		数组包含(全局.b, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) == 真;
		在视线内(事件玩家, 距离准星最近的玩家(事件玩家, 所有队伍), 敌方屏障阻挡视线) == 真;
	}

	动作
	{
		伤害(距离准星最近的玩家(事件玩家, 所有队伍), 事件玩家, 550);
		播放效果(所有玩家(所有队伍), 负面状态施加声音, 白色, 事件玩家, 120);
		播放效果(所有玩家(所有队伍), 有害爆炸, 白色, 事件玩家, 8);
		小字体信息(事件玩家, 自定义字符串("已击倒望着的感染体,深渊凝视进入冷却20秒"));
		If(距离准星最近的玩家(事件玩家, 所有队伍) != 全局.P);
			设置状态(距离准星最近的玩家(事件玩家, 所有队伍), 事件玩家, 击倒, 3);
		Else;
			设置状态(距离准星最近的玩家(事件玩家, 所有队伍), 事件玩家, 击倒, 1);
		End;
		等待(20, 无视条件);
		小字体信息(事件玩家, 自定义字符串("深渊凝视冷却完成,可再次释放"));
		播放效果(事件玩家, 环状爆炸声音, 白色, 事件玩家, 100);
	}
}

