	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		禅雅塔;
	}

	条件
	{
		事件玩家.XUANZHE == 2;
		存活(事件玩家) == 真;
		按钮被按下(事件玩家, 技能2) == 真;
		(数组包含(全局.b, 所用英雄(距离准星最近的玩家(事件玩家, 所有队伍))) && 数组包含(范围内玩家(事件玩家, 8, 所有队伍, 关闭), 距离准星最近的玩家(事件玩家, 所有队伍))) == 真;
	}

	动作
	{
		事件玩家.GONGYONG = 距离准星最近的玩家(事件玩家, 所有队伍);
		播放效果(所有玩家(所有队伍), 有害选择效果 , 黄色, 事件玩家.GONGYONG, 3);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 100);
		设置启用技能 2(事件玩家, 假);
		施加推力(事件玩家, 地图矢量(矢量(0, 1, -1), 事件玩家, 旋转), 28, 至地图, 取消相反运动);
		设置状态(事件玩家.GONGYONG, 事件玩家, 昏迷, 1.500);
		等待(6, 无视条件);
		设置启用技能 2(事件玩家, 真);
	}
}

规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		禅雅塔;
	}

	条件
	{
		事件玩家.XUANZHE > 1;
		按钮被按下(事件玩家, 互动) == 真;
	}

	动作
	{
		小字体信息(事件玩家, 自定义字符串("已更改被动,技能进入冷却10秒"));
		播放效果(所有玩家(所有队伍), 有害选择效果 , 黄色, 事件玩家, 3);
		播放效果(所有玩家(所有队伍), 环状爆炸声音, 白色, 事件玩家, 100);
		If(事件玩家.XUANZHE == 2);
			事件玩家.XUANZHE = 3;
			事件玩家.WENzi = 自定义字符串(
				"切换(主动):将自己的被动切换成另外二种\r\n切换后会使自己加速2秒,冷却10秒\r\n{0}大招结束后对周围感染体造成高额伤害\r\n普渡(被动):比赛时间<70秒,蹲下6秒可\r\n提前将逃生通道召唤至自己脚下位置", 技能图标字符串(英雄(
				禅雅塔), 按钮(终极技能)));
		Else;
			事件玩家.XUANZHE = 2;
			事件玩家.WENzi = 自定义字符串("切换(主动):将自己的被动切换成另外二种\r\n切换后会使自己加速2秒,冷却10秒\r\n{0}近距离对感染体使用,对其晕眩1.5秒\r\n并使自身后退\r\n{1}使自己加速80%持续2秒", 技能图标字符串(英雄(
				禅雅塔), 按钮(技能2)), 技能图标字符串(英雄(禅雅塔), 按钮(技能1)));
		End;
		设置移动速度(事件玩家, 事件玩家.SH + 200);
		等待(2, 无视条件);
		设置移动速度(事件玩家, 事件玩家.SH + 100);
		等待(8, 无视条件);
	}
}

