			击杀(事件玩家, 无);
		Else;
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 30);
			等待(14.900, 无视条件);
			事件玩家.GONGJI += 15;
			事件玩家.SM += 10;
			事件玩家.SH += 4;
			等待(0.100, 无视条件);
			设置造成伤害(事件玩家, 事件玩家.GONGJI + 100);
			设置移动速度(事件玩家, 事件玩家.SH + 100);
