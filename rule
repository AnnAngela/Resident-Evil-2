规则("")
{
	事件
	{
		持续 - 每名玩家;
		双方;
		全部;
	}

	条件
	{
		全局.AOLS == 真;
		事件玩家 == 全局.N;
		按钮被按下(事件玩家, 近身攻击) == 真;
		数组包含(范围内玩家(事件玩家, 5, 所有队伍, 关闭), 距离最近的玩家(事件玩家, 所有队伍)) == 真;
		距离最近的玩家(事件玩家, 所有队伍) != 全局.P;
		具有状态(事件玩家, 击倒) != 真;
	}

	动作
	{
		If(距离最近的玩家(事件玩家, 所有队伍) != 全局.P);
			小字体信息(事件玩家, 自定义字符串("你已将病毒转移"));
			全局.N = 距离最近的玩家(事件玩家, 所有队伍);
			设置状态(全局.N, 事件玩家, 击倒, 2);
			设置引力(全局.N, 100);
			等待(0.100, 无视条件);
			小字体信息(所有玩家(所有队伍), 字符串("{0} {1} {2}", 所用英雄(事件玩家), 自定义字符串("已将病毒转移给了"), 所用英雄(全局.N)));
		Else;
			小字体信息(事件玩家, 自定义字符串("对方是母体,你不能将病毒转移到母体身上"));
		End;
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		死亡(全局.P) == 真;
	}

	动作
	{
		If(比赛时间 > 340);
			大字体信息(所有玩家(所有队伍), 自定义字符串("母体死亡太早,不算"));
		End;
		等待(7.500, 无视条件);
		设置状态(全局.P, 无, 消散, 1.300);
		If(所用英雄(全局.P) == 英雄(D.Va));
			设置终极技能充能(全局.P, 100);
			按下按键(全局.P, 终极技能);
		Else If(所用英雄(全局.P) == 英雄(温斯顿));
			按下按键(全局.P, 终极技能);
		End;
		等待(0.250, 无视条件);
		设置最大生命值(全局.P, 全局.sm);
	}
}

规则("")
{
	事件
	{
		持续 - 全局;
	}

	条件
	{
		游戏正在进行中 == 真;
		全局.K == 0;
		比赛时间 < 375;
	}

